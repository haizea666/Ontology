      <xsd:appinfo>
        <app:SemanticID>MIM:c21d7ff9-97ee-4743-bd15-7242d2c5150b</app:SemanticID>
        <app:DisplayName>Non-Friendly Order of Battle</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="NonFriendlyORBATType">
