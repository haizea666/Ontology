        <app:ShortName>typeName</app:ShortName>
        <app:Remarks>Possible ORBAT type names include 'Black' for criminal forces, 'Green' for local national forces, 'Red' for enemy forces, or 'White' for civilian organisations and NGOs.</app:Remarks>
        <app:ValueOrAmplification>1</app:ValueOrAmplification>
        <app:MIMStereotype>instance</app:MIMStereotype>
        <app:MIMStereotype>name</app:MIMStereotype>
        <app:DisplayName>type</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
