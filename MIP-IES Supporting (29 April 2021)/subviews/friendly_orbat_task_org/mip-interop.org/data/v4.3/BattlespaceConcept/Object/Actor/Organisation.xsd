    <xsd:annotation>
      <xsd:documentation xml:lang="en">The subject Organisation is functionally related to the object Organisation.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:ad74baaa-8b87-417c-b844-fe3641a3418f-2R</app:SemanticID>
        <app:ShortName>functionalObjectOrganisationRef</app:ShortName>
        <app:MIMStereotype>instance</app:MIMStereotype>
        <app:MIMStereotype>structure</app:MIMStereotype>
        <app:ReferenceType>MIM:9d29c033-a496-41e6-b6a5-3b610335cded</app:ReferenceType>
        <app:DisplayName>is functionally related to</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="OrganisationFunctionalAssociationFunctionalSubjectOrganisation" type="organisation:MilitaryOrganisationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The subject Organisation is functionally related to the object Organisation.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:ad74baaa-8b87-417c-b844-fe3641a3418f-1I</app:SemanticID>
        <app:ShortName>functionalSubjectOrganisation</app:ShortName>
        <app:MIMStereotype>instance</app:MIMStereotype>
        <app:MIMStereotype>structure</app:MIMStereotype>
        <app:DisplayName>is functionally related to</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="OrganisationFunctionalAssociationFunctionalSubjectOrganisationRef" type="base:IdentifiableReferenceType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The subject Organisation is functionally related to the object Organisation.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:ad74baaa-8b87-417c-b844-fe3641a3418f-1R</app:SemanticID>
        <app:ShortName>functionalSubjectOrganisationRef</app:ShortName>
        <app:MIMStereotype>instance</app:MIMStereotype>
        <app:MIMStereotype>structure</app:MIMStereotype>
        <app:ReferenceType>MIM:9d29c033-a496-41e6-b6a5-3b610335cded</app:ReferenceType>
        <app:DisplayName>is functionally related to</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="OrganisationFunctionalAssociationType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The nature of functional relationship between the subject and object Organisations.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:c959cb55-5877-4cdd-b45c-9ac692b6da07</app:SemanticID>
        <app:MIMStereotype>Abstract</app:MIMStereotype>
        <app:DisplayName>Organisation functional association</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="base:AssociationType">
        <xsd:sequence>
          <xsd:element ref="organisation:OrganisationFunctionalAssociationFunctionalObjectOrganisationRef" minOccurs="0" />
          <xsd:element ref="organisation:OrganisationFunctionalAssociationFunctionalObjectOrganisation" minOccurs="0" />
          <xsd:element ref="organisation:OrganisationFunctionalAssociationFunctionalSubjectOrganisationRef" minOccurs="0" />
          <xsd:element ref="organisation:OrganisationFunctionalAssociationFunctionalSubjectOrganisation" minOccurs="0" />
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="OrganisationHasCommandFunctionIndicator" type="organisation:OrganisationHasCommandFunctionIndicatorType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">Whether the Organisation has a command function (true) or not (false).</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:3ccc1ec1-bf33-44fa-ad9c-a2b6d68fa528</app:SemanticID>
