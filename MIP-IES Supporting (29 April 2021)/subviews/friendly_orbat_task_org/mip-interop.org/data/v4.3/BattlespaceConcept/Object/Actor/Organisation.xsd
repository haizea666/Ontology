      <xsd:appinfo>
        <app:SemanticID>MIM:ad74baaa-8b87-417c-b844-fe3641a3418f-2I</app:SemanticID>
        <app:ShortName>functionalObjectOrganisation</app:ShortName>
        <app:MIMStereotype>instance</app:MIMStereotype>
        <app:MIMStereotype>structure</app:MIMStereotype>
        <app:DisplayName>is functionally related to</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
