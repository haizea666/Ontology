<?xml version="1.0"?>
<TestCase xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://mip-interop.org/schemas/SEPT/2016/09/29/TestAutomation.xsd">
  <TestCaseId>
    <TestCaseIdBlock>MIP4</TestCaseIdBlock>
    <TestCaseIdLevel>SLT3IS</TestCaseIdLevel>
    <TestCaseIdGroup>52</TestCaseIdGroup>
    <TestCaseIdNumber>22</TestCaseIdNumber>
  </TestCaseId>
  <TestCaseTitle>Add CBRNControlFeature Sub-Types.</TestCaseTitle>
  <TestCaseExecutionPriority>1</TestCaseExecutionPriority>
  <TestCaseObjective>BSO validation. Verify the ability to create and add several CBRNControlFeatures (CBRNControlFeature Sub-Types) in a nation's C2IS.</TestCaseObjective>
  <TestCaseScenario>Nation A creates several CBRNControlFeatures. Nation B receives the information and interprets it correctly.</TestCaseScenario>
  <TestCaseEnvironment>Internet or Co-located. Exchange Pattern: R/R, P/S.</TestCaseEnvironment>
  <TestCaseMaximumParticipation>2</TestCaseMaximumParticipation>
  <TestCasePreCondition>Nation A has created a Topic which includes CBRNControlFeature Sub-Types message elements, for instance Ground Picture &gt; CBRN.
Nation B has subscribed to this Topic.</TestCasePreCondition>
  <TestCaseInput>N/A.</TestCaseInput>
  <TestCaseConclusion>This test is considered a success if the created CBRNControlFeatures are displayed correctly on both Nations' C2IS.</TestCaseConclusion>
  <TestCaseOutput>N/A.</TestCaseOutput>
  <TestCaseTraceability>REQ_INF_0002</TestCaseTraceability>
  <TestCaseStep>
    <TestCaseStepNumber>1</TestCaseStepNumber>
    <TestCaseStepAction>Nation A creates a CBRNHazardArea </TestCaseStepAction>
    <TestCaseStepResult>Nation B receives and displays the CBRNHazardArea correctly. </TestCaseStepResult>
    <TestCaseStepDestination>2</TestCaseStepDestination>
    <TestCaseStepSource>1</TestCaseStepSource>
  </TestCaseStep>
  <TestCaseStep>
    <TestCaseStepNumber>2</TestCaseStepNumber>
    <TestCaseStepAction>Nation A creates a BiologicallyContaminatedArea </TestCaseStepAction>
    <TestCaseStepResult>Nation B receives and displays the BiologicallyContaminatedArea correctly. </TestCaseStepResult>
    <TestCaseStepDestination>2</TestCaseStepDestination>
    <TestCaseStepSource>1</TestCaseStepSource>
  </TestCaseStep>
  <TestCaseStep>
    <TestCaseStepNumber>3</TestCaseStepNumber>
    <TestCaseStepAction>Nation A creates a CBRNReleaseOrAttackArea </TestCaseStepAction>
    <TestCaseStepResult>Nation B receives and displays the CBRNReleaseOrAttackArea correctly. </TestCaseStepResult>
    <TestCaseStepDestination>2</TestCaseStepDestination>
    <TestCaseStepSource>1</TestCaseStepSource>
  </TestCaseStep>
  <TestCaseStep>
    <TestCaseStepNumber>4</TestCaseStepNumber>
    <TestCaseStepAction>Nation A creates a CBRNZoneI </TestCaseStepAction>
    <TestCaseStepResult>Nation B receives and displays the CBRNZoneI correctly. </TestCaseStepResult>
    <TestCaseStepDestination>2</TestCaseStepDestination>
    <TestCaseStepSource>1</TestCaseStepSource>
  </TestCaseStep>
  <TestCaseStep>
    <TestCaseStepNumber>5</TestCaseStepNumber>
    <TestCaseStepAction>Nation A creates a ChemicalSurfaceDepositionConcentrationContourLine </TestCaseStepAction>
    <TestCaseStepResult>Nation B receives and displays the ChemicalSurfaceDepositionConcentrationContourLine correctly. </TestCaseStepResult>
    <TestCaseStepDestination>2</TestCaseStepDestination>
    <TestCaseStepSource>1</TestCaseStepSource>
  </TestCaseStep>
  <TestCaseStep>
    <TestCaseStepNumber>6</TestCaseStepNumber>
    <TestCaseStepAction>Nation A creates a MinimumSafeDistance1 </TestCaseStepAction>
    <TestCaseStepResult>Nation B receives and displays the MinimumSafeDistance1 correctly. </TestCaseStepResult>
    <TestCaseStepDestination>2</TestCaseStepDestination>
    <TestCaseStepSource>1</TestCaseStepSource>
  </TestCaseStep>
  <TestCaseStep>
    <TestCaseStepNumber>7</TestCaseStepNumber>
    <TestCaseStepAction>Nation A creates a NuclearTargetType </TestCaseStepAction>
    <TestCaseStepResult>Nation B receives and displays the NuclearTargetType correctly. </TestCaseStepResult>
    <TestCaseStepDestination>2</TestCaseStepDestination>
    <TestCaseStepSource>1</TestCaseStepSource>
  </TestCaseStep>
  <TestCaseStep>
    <TestCaseStepNumber>8</TestCaseStepNumber>
    <TestCaseStepAction>Nation A creates a RadioactiveArea </TestCaseStepAction>
    <TestCaseStepResult>Nation B receives and displays the RadioactiveArea correctly. </TestCaseStepResult>
    <TestCaseStepDestination>2</TestCaseStepDestination>
    <TestCaseStepSource>1</TestCaseStepSource>
  </TestCaseStep>
  <TestCaseEventGeneration>true</TestCaseEventGeneration>
</TestCase>