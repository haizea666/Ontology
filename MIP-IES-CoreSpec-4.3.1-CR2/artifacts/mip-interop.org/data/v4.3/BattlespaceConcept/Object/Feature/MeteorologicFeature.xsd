<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:app="https://mip-interop.org/data/v4.3/AppInfo" xmlns:base="https://mip-interop.org/data/v4.3/Base" xmlns:feature="https://mip-interop.org/data/v4.3/BattlespaceConcept/Object/Feature" xmlns:generic="https://mip-interop.org/data/v4.3/BattlespaceConcept/Generic" xmlns:meteorologicfeature="https://mip-interop.org/data/v4.3/BattlespaceConcept/Object/Feature/MeteorologicFeature" xmlns:object="https://mip-interop.org/data/v4.3/BattlespaceConcept/Object" xmlns:primitives="https://mip-interop.org/data/v4.3/Primitives" targetNamespace="https://mip-interop.org/data/v4.3/BattlespaceConcept/Object/Feature/MeteorologicFeature" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
  <xsd:import schemaLocation="https://mip-interop.org/data/v4.3/BattlespaceConcept/Object/Feature.xsd" namespace="https://mip-interop.org/data/v4.3/BattlespaceConcept/Object/Feature" />
  <xsd:import schemaLocation="https://mip-interop.org/data/v4.3/BattlespaceConcept/Generic.xsd" namespace="https://mip-interop.org/data/v4.3/BattlespaceConcept/Generic" />
  <xsd:import schemaLocation="https://mip-interop.org/data/v4.3/Base.xsd" namespace="https://mip-interop.org/data/v4.3/Base" />
  <xsd:import schemaLocation="https://mip-interop.org/data/v4.3/BattlespaceConcept/Object.xsd" namespace="https://mip-interop.org/data/v4.3/BattlespaceConcept/Object" />
  <xsd:import schemaLocation="https://mip-interop.org/data/v4.3/Primitives.xsd" namespace="https://mip-interop.org/data/v4.3/Primitives" />
  <xsd:import schemaLocation="https://mip-interop.org/data/v4.3/AppInfo.xsd" namespace="https://mip-interop.org/data/v4.3/AppInfo" />
  <xsd:annotation>
    <xsd:documentation xml:lang="en">The Schema.</xsd:documentation>
    <xsd:appinfo />
  </xsd:annotation>
  <xsd:element name="Atmosphere" type="meteorologicfeature:AtmosphereType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The mixture of gases that surround the earth, characterized by humidity, pressure, and temperature.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:05e99e56-3bb2-4387-a9ee-5e16e1341fe9</app:SemanticID>
        <app:Geometry app:semanticID="MIM:1b27d622-6f58-406d-aceb-2402af195155">AbsolutePoint</app:Geometry>
        <app:DisplayName>Atmosphere</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AtmosphereHumidity" type="meteorologicfeature:HumidityType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The moisture in the Atmosphere.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:5e39dc16-000f-4aad-af12-6199a70945e9</app:SemanticID>
        <app:ShortName>humidity</app:ShortName>
        <app:MIMStereotype>instance</app:MIMStereotype>
        <app:MIMStereotype>structure</app:MIMStereotype>
        <app:DisplayName>humidity</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AtmosphereInversionLayerCode" type="meteorologicfeature:AtmosphereInversionLayerCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The height of the inversion layer in the Atmosphere. The stability class describes the degree of mixing of released material in the Atmosphere.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:01b8d3ad-23b9-4154-8de3-26f6d243cfb3</app:SemanticID>
        <app:ShortName>inversionLayerCode</app:ShortName>
        <app:MIMStereotype>code</app:MIMStereotype>
        <app:MIMStereotype>instance</app:MIMStereotype>
        <app:DisplayName>inversion layer</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="AtmosphereInversionLayerCodeType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The height of the inversion layer in the atmosphere.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:92356b4b-93f8-4450-8071-3591ec1c383c</app:SemanticID>
        <app:MIMStereotype>complete</app:MIMStereotype>
        <app:MIMStereotype>ordered</app:MIMStereotype>
        <app:OrderRelation>is lower altitude than</app:OrderRelation>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="primitives:CodeValueType">
      <xsd:enumeration value="_200Metres">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Top of inversion layer lower than 200 metres above ground.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:ec6ea396-83b2-42a4-b316-77b52ce2df7b</app:SemanticID>
            <app:DisplayName>C - 200 metres</app:DisplayName>
            <app:PhysicalName>C</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="_400Metres">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Top of inversion layer lower than 400 metres above ground.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:adb495b6-b406-4201-919b-9ed92823eb1e</app:SemanticID>
            <app:DisplayName>B - 400 metres</app:DisplayName>
            <app:PhysicalName>B</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="_800Metres">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Top of inversion layer lower than 800 metres above ground.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:b174a6a1-8b92-4c8a-a935-d9153f1da02d</app:SemanticID>
            <app:DisplayName>A - 800 metres</app:DisplayName>
            <app:PhysicalName>A</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:element name="AtmospherePressureMeasure" type="meteorologicfeature:AtmospherePressureMeasureType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The ambient air pressure in terms of force per unit area.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:5f7f561a-c78a-4160-b37b-2df09547610e</app:SemanticID>
        <app:ShortName>pressureMeasure</app:ShortName>
        <app:MIMStereotype>instance</app:MIMStereotype>
        <app:MIMStereotype>measure</app:MIMStereotype>
        <app:DisplayName>pressure</app:DisplayName>
        <app:UnitOfMeasure>pascal (Pa)</app:UnitOfMeasure>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="AtmospherePressureMeasureType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The ambient air pressure in terms of force per unit area.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:5f7f561a-c78a-4160-b37b-2df09547610e</app:SemanticID>
        <app:ShortName>pressureMeasure</app:ShortName>
        <app:MIMStereotype>instance</app:MIMStereotype>
        <app:MIMStereotype>measure</app:MIMStereotype>
        <app:DisplayName>pressure</app:DisplayName>
        <app:UnitOfMeasure>pascal (Pa)</app:UnitOfMeasure>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="primitives:DecimalType">
      <xsd:minExclusive value="0" />
      <xsd:fractionDigits value="1" />
      <xsd:totalDigits value="7" />
      <xsd:maxInclusive value="999999.9" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:element name="AtmospherePressureSystemCategoryCode" type="meteorologicfeature:AtmospherePressureSystemCategoryCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The class of a pressure system in the Atmosphere.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:9c095fdb-35ec-490e-a703-e27b2ce702ac</app:SemanticID>
        <app:ShortName>pressureSystemCategoryCode</app:ShortName>
        <app:MIMStereotype>code</app:MIMStereotype>
        <app:MIMStereotype>instance</app:MIMStereotype>
        <app:DisplayName>pressure system category</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="AtmospherePressureSystemCategoryCodeType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The class of pressure system used to qualify an Atmosphere.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:e671d545-533b-4bf4-84ac-e5d5158692e6</app:SemanticID>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="primitives:CodeValueType">
      <xsd:enumeration value="ColdFront">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The discontinuity at the forward edge of an advancing cold air mass that is displacing warmer air in its path.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:81e2d3d7-8360-447e-9060-a836896805f9</app:SemanticID>
            <app:DisplayName>Cold Front</app:DisplayName>
            <app:PhysicalName>CLDFRT</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="HighPressureCentre">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">An area of high atmospheric pressure that has a closed circulation that is anticyclonic, i.e., as viewed from above, the circulation is clockwise in the Northern Hemisphere, counterclockwise in the southern Hemisphere, undefined at the Equator.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:2ba3b9c3-a3c2-4e35-88a3-380b1e145aa1</app:SemanticID>
            <app:DisplayName>High pressure centre</app:DisplayName>
            <app:PhysicalName>HGHPRS</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="LowPressureCentre">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">1. An area of low atmospheric pressure that has a closed circulation that is cyclonic, i.e., as viewed from above, the circulation is counterclockwise in the Northern Hemisphere, clockwise in the Southern Hemisphere, undefined at the Equator. Because cyclonic circulation and relatively low atmospheric pressure usually coexist, in common practice the terms cyclone and low are used interchangeably. Also, because cyclones often are accompanied by inclement (sometimes destructive) weather, they are frequently referred to simply as storms. 2. Frequently misused to denote a tornado. 3. In the Indian Ocean, a tropical cyclone of hurricane or typhoon force.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:dfab655d-a6a7-41a3-a1fc-bcac4ca32f5b</app:SemanticID>
            <app:DisplayName>Low pressure centre</app:DisplayName>
            <app:PhysicalName>LOWPRS</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="OccludedFront">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The line along which a cold front has overtaken a warm front at ground level.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:508fcf58-b310-4a56-b01c-dbcd99328484</app:SemanticID>
            <app:DisplayName>Occluded front</app:DisplayName>
            <app:PhysicalName>OCCFRT</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="RidgeLine">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">An elongated region of high atmospheric pressure.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:ae0f0f0a-9a7c-4615-bbcf-636ccdbd6d4d</app:SemanticID>
            <app:DisplayName>Ridge line</app:DisplayName>
            <app:PhysicalName>RDGLIN</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="SquallLine">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A line of high winds and thunderstorms in convectively unstable air, an instability line (of non-frontal nature); it may be generated by a cold front.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:37df63d4-7315-48a5-9224-1ddad4248cf3</app:SemanticID>
            <app:DisplayName>Squall line</app:DisplayName>
            <app:PhysicalName>SQLLIN</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="StationaryFront">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A situation in which the surface position of a front does not move; the flow on either side of such a boundary is nearly parallel to the position of the front.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:ab75d1a3-3731-40d2-b850-db8333570019</app:SemanticID>
            <app:DisplayName>Stationary front</app:DisplayName>
            <app:PhysicalName>STAFRT</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="TroughLine">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">An elongated region of low atmospheric pressure.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:27f8b027-a307-444f-b03f-7fee0c691b24</app:SemanticID>
            <app:DisplayName>Trough line</app:DisplayName>
            <app:PhysicalName>TRGLIN</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="UpperColdFront">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Occurs when discontinuity at the forward edge of an advancing cold air mass is displacing warmer air in its path and the two air masses intersect above ground level.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:98dc87a6-a812-4f93-a5d0-18d9783e1646</app:SemanticID>
            <app:DisplayName>Upper cold front</app:DisplayName>
            <app:PhysicalName>UPCFRT</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="UpperWarmFront">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Occurs when discontinuity at the forward edge of an advancing warm air mass is displacing cooler air in its path and the two air masses intersect above ground level.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:9e898f9a-6a48-42c4-b95a-056887ffc9d1</app:SemanticID>
            <app:DisplayName>Upper warm front</app:DisplayName>
            <app:PhysicalName>UPWFRT</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="WarmFront">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The discontinuity at the forward edge of an advancing warm air mass that is displacing cooler air in its path.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:331b5850-1489-44fe-a2f0-924c39db876d</app:SemanticID>
            <app:DisplayName>Warm front</app:DisplayName>
            <app:PhysicalName>WRMFRT</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:element name="AtmosphereTemperature" type="meteorologicfeature:AtmosphereTemperatureType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The heat of the ambient air in the Atmosphere.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:aed1ce03-c3f3-4deb-b4e8-0bc413e51a71</app:SemanticID>
        <app:ShortName>temperature</app:ShortName>
        <app:MIMStereotype>instance</app:MIMStereotype>
        <app:MIMStereotype>temperature</app:MIMStereotype>
        <app:DisplayName>temperature</app:DisplayName>
        <app:UnitOfMeasure>degree Celsius (°C)</app:UnitOfMeasure>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AtmosphereTemperatureGradientCode" type="meteorologicfeature:AtmosphereTemperatureGradientCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The heat change with respect to the ground and 100 m in elevation in a certain area. Acts as an indication of vertical air movement between the ground and higher elevations.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:d5579a85-359c-4254-be9d-3ef25bb2f679</app:SemanticID>
        <app:ShortName>temperatureGradientCode</app:ShortName>
        <app:MIMStereotype>code</app:MIMStereotype>
        <app:MIMStereotype>instance</app:MIMStereotype>
        <app:DisplayName>temperature gradient</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="AtmosphereTemperatureGradientCodeType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The heat change with respect to the ground and 100 m in elevation in a certain area. Acts as an indication of vertical air movement between the ground and higher elevations.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:3e469e9e-ea1c-45b0-9a04-843bf36f3c4b</app:SemanticID>
        <app:MIMStereotype>complete</app:MIMStereotype>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="primitives:CodeValueType">
      <xsd:enumeration value="Neutral">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A one degree Kelvin, or less, temperature gradient between the ground and 100 metres above ground level.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:68383ba3-be8e-4602-bc5b-f83e29d9ee2f</app:SemanticID>
            <app:DisplayName>Neutral</app:DisplayName>
            <app:PhysicalName>NEUTRL</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Stable">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">More than one degree Kelvin positive temperature gradient between the ground and 100 metres above ground level. (colder lower layers)</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:4802860e-4bd1-45ff-a363-0ca368d137dc</app:SemanticID>
            <app:DisplayName>Stable</app:DisplayName>
            <app:PhysicalName>STABLE</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Unstable">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">More than one degree Kelvin negative temperature gradient between the ground and 100 metres above ground level. (hotter lower layers)</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:7428fd91-38f8-4ed0-a4e0-123a9f31c969</app:SemanticID>
            <app:DisplayName>Unstable</app:DisplayName>
            <app:PhysicalName>UNSTAB</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AtmosphereTemperatureType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The heat of the ambient air in the Atmosphere.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:aed1ce03-c3f3-4deb-b4e8-0bc413e51a71</app:SemanticID>
        <app:ShortName>temperature</app:ShortName>
        <app:MIMStereotype>instance</app:MIMStereotype>
        <app:MIMStereotype>temperature</app:MIMStereotype>
        <app:DisplayName>temperature</app:DisplayName>
        <app:UnitOfMeasure>degree Celsius (°C)</app:UnitOfMeasure>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="primitives:DecimalType">
      <xsd:minInclusive value="-273.1" />
      <xsd:fractionDigits value="1" />
      <xsd:totalDigits value="5" />
      <xsd:maxInclusive value="9999.9" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="AtmosphereType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The mixture of gases that surround the earth, characterized by humidity, pressure, and temperature.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:05e99e56-3bb2-4387-a9ee-5e16e1341fe9</app:SemanticID>
        <app:Geometry app:semanticID="MIM:1b27d622-6f58-406d-aceb-2402af195155">AbsolutePoint</app:Geometry>
        <app:DisplayName>Atmosphere</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="meteorologicfeature:MeteorologicFeatureType">
        <xsd:sequence>
          <xsd:element ref="meteorologicfeature:AtmosphereHumidity" minOccurs="0" />
          <xsd:element ref="meteorologicfeature:AtmosphereInversionLayerCode" minOccurs="0" />
          <xsd:element ref="meteorologicfeature:AtmospherePressureMeasure" minOccurs="0" />
          <xsd:element ref="meteorologicfeature:AtmospherePressureSystemCategoryCode" minOccurs="0" />
          <xsd:element ref="meteorologicfeature:AtmosphereTemperature" minOccurs="0" />
          <xsd:element ref="meteorologicfeature:AtmosphereTemperatureGradientCode" minOccurs="0" />
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="AverageCoverageInOktaCode" type="meteorologicfeature:AverageCoverageInOktaCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The average density.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:b9bf245e-aafc-4e72-9ccd-6173172c6bc6</app:SemanticID>
        <app:ShortName>code</app:ShortName>
        <app:ValueOrAmplification>1</app:ValueOrAmplification>
        <app:MIMStereotype>code</app:MIMStereotype>
        <app:MIMStereotype>discriminator</app:MIMStereotype>
        <app:MIMStereotype>instance</app:MIMStereotype>
        <app:DisplayName>code</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="AverageCoverageInOktaCodeType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The average density of a CloudCover as fractional coverage.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:001aaed9-b842-4c5b-8e46-387eecbc7af9</app:SemanticID>
        <app:MIMStereotype>complete</app:MIMStereotype>
        <app:MIMStereotype>ordered</app:MIMStereotype>
        <app:OrderRelation>is less coverage than</app:OrderRelation>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="primitives:CodeValueType">
      <xsd:enumeration value="_0Okta">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Clear sky.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:90fa2d9a-d06d-49f0-9870-008e039bf3ed</app:SemanticID>
            <app:DisplayName>0/8</app:DisplayName>
            <app:PhysicalName>0</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="_1Okta">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Sky partially obscured 12 ½ percent.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:cdcff91b-62ef-422f-94dd-9168f22a3f71</app:SemanticID>
            <app:DisplayName>1/8</app:DisplayName>
            <app:PhysicalName>1</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="_2Okta">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Sky partially obscured 25 percent.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:d4e7dc4c-476b-4568-bd85-f0f14f0a24aa</app:SemanticID>
            <app:DisplayName>2/8</app:DisplayName>
            <app:PhysicalName>2</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="_3Okta">
