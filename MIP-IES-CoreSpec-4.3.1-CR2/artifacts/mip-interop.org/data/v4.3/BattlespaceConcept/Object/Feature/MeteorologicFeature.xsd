            <app:DisplayName>Absolute minimum</app:DisplayName>
            <app:PhysicalName>ABMIN</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AverageMaximum">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The average of maximum readings over a period of time.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:d3267e26-ef5a-441c-a5a8-206a8478af51</app:SemanticID>
            <app:DisplayName>Average maximum</app:DisplayName>
            <app:PhysicalName>AVMAX</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AverageMinimum">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The average of minimum readings over a period of time.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:c913827c-414d-47ac-921d-9b4aaa480690</app:SemanticID>
            <app:DisplayName>Average minimum</app:DisplayName>
            <app:PhysicalName>AVMIN</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Nominal">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The actual or expected value.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:0d0be047-c7f3-48f1-a165-442f03a7e134</app:SemanticID>
            <app:DisplayName>Nominal</app:DisplayName>
            <app:PhysicalName>NOMIN</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:element name="MeteorologicFeatureProbabilityPercent" type="meteorologicfeature:MeteorologicFeatureProbabilityPercentType" nillable="true">
