  </xsd:element>
  <xsd:simpleType name="AirControlFeatureCategoryCodeType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The class of AirControlFeature.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:1204b304-d62e-4f4d-94f7-3d3eda937264</app:SemanticID>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="primitives:CodeValueType">
      <xsd:enumeration value="AirAxisOfAdvance">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A general air corridor of advance, which extends towards the enemy. An air axis of advance symbol graphically portrays a commander’s air manoeuvre intention, such as avoidance air defence coverage or envelopment of an enemy force. It follows an axis suitable for the airmobile force to which the axis was assigned. An air axis of advance does not directly control or use terrain.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:718c4582-096e-42f7-96e9-e100d51c68de</app:SemanticID>
            <app:Geometry app:semanticID="MIM:0f665499-bf36-4b91-b752-923e34fe7869">MultiPointCategoryCode::Arrow3To50Points</app:Geometry>
            <app:DisplayName>Air axis of advance</app:DisplayName>
            <app:PhysicalName>AAXIS</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AirContactPoint">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A position at which a mission leader makes radio contact with an air control agency. Note: A point used for control purposes in air-to-air refuelling and close air support missions.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:6b9ab44e-cefd-4622-b352-27bdbfd5cfb6</app:SemanticID>
            <app:Geometry app:semanticID="MIM:1b27d622-6f58-406d-aceb-2402af195155">AbsolutePoint</app:Geometry>
            <app:DisplayName>Contact point, air</app:DisplayName>
            <app:PhysicalName>CNTPTA</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AirControlPoint">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">An easily identifiable point on the terrain or an electronic navigational aid used to provide necessary control during air movement. Air control points are generally designated at each point where the flight route or air corridor makes a definite change in any direction and at any other point deemed necessary for timing or control of the operation.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:d09526cc-2e95-4b6a-84b2-3df5cd62b743</app:SemanticID>
            <app:Geometry app:semanticID="MIM:1b27d622-6f58-406d-aceb-2402af195155">AbsolutePoint</app:Geometry>
            <app:DisplayName>Air control point</app:DisplayName>
            <app:PhysicalName>AIRCTP</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AirControlRendezvous">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">[No definition provided by the external source.]</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:05d5b788-fd5c-47da-b47b-44376df622c6</app:SemanticID>
            <app:Geometry app:semanticID="MIM:1b27d622-6f58-406d-aceb-2402af195155">AbsolutePoint</app:Geometry>
            <app:DisplayName>Air control rendezvous</app:DisplayName>
            <app:PhysicalName>AIRRV</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AirDefenceActionArea">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">An area and the airspace above it within which friendly aircraft or surface-to-air weapons are normally given precedence in operations except under specified conditions.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:cd68fbc7-8185-4517-9fad-eba4b29ec00b</app:SemanticID>
            <app:Geometry app:semanticID="MIM:a9198101-9748-4baf-9818-f303f38ed6be">PolygonArea</app:Geometry>
            <app:DisplayName>Air defence action area</app:DisplayName>
            <app:PhysicalName>ADACAR</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AirDefenceIdentificationZone">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Airspace within which ready identification, location, and control of airborne vehicles are required.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:bdd986f1-8b1f-41c6-9c39-a64070aab517</app:SemanticID>
            <app:Geometry app:semanticID="MIM:a9198101-9748-4baf-9818-f303f38ed6be">PolygonArea</app:Geometry>
            <app:DisplayName>Air defence identification zone</app:DisplayName>
            <app:PhysicalName>ADIDZN</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AirPolicingArea">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A ControlFeature that represents an area assigned to interceptor aircraft in peacetime, for the purpose of preserving the integrity of a specified airspace.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:4ca82e65-58c1-41a5-a3a8-a3651b0dd11d</app:SemanticID>
            <app:Geometry app:semanticID="MIM:a9198101-9748-4baf-9818-f303f38ed6be">PolygonArea</app:Geometry>
            <app:DisplayName>Air policing area</app:DisplayName>
            <app:PhysicalName>APA</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AirReferencePoint">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A point used in air operations for referencing geographic position.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:537c7a69-e569-45bc-acda-534f0f0c3821</app:SemanticID>
            <app:Geometry app:semanticID="MIM:1b27d622-6f58-406d-aceb-2402af195155">AbsolutePoint</app:Geometry>
            <app:DisplayName>Air reference point</app:DisplayName>
            <app:PhysicalName>AIRREF</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AirRefuellingControlPoint">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A ControlFeature that represents a geographical position over which the receiver(s) arrive(s) in the observation/precontact position with respect to the tanker, using its name, Lat/Long minutes or its bearing and distance from reference point.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:6b473451-9b98-440d-af7c-36465d72265a</app:SemanticID>
            <app:Geometry app:semanticID="MIM:1b27d622-6f58-406d-aceb-2402af195155">AbsolutePoint</app:Geometry>
            <app:DisplayName>Air refuelling control point</app:DisplayName>
            <app:PhysicalName>ARCP</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AirSurveillanceArea">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A ControlFeature that represents an area designated to a systematic observation by any means, for the purpose of identifying aircraft and or missiles (friendly and/or enemy), and determining their trajectories.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:7febb630-af90-410d-8e61-3b8c97f45b6d</app:SemanticID>
            <app:Geometry app:semanticID="MIM:a9198101-9748-4baf-9818-f303f38ed6be">PolygonArea</app:Geometry>
            <app:DisplayName>Air surveillance area</app:DisplayName>
            <app:PhysicalName>ASA</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AirToAirRefuellingArea">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Airspace of defined dimensions set aside for air-to-air refuelling operations, excluding special operation forces air-to-air missions.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:0829b6a8-85ae-4149-a928-19421ecf1e03</app:SemanticID>
            <app:Geometry app:semanticID="MIM:c99353bf-0f8d-4390-8d7b-e1a8ea62b818;MIM:a9198101-9748-4baf-9818-f303f38ed6be">SurfaceVolume;PolygonArea</app:Geometry>
            <app:DisplayName>Air-to-air refuelling area</app:DisplayName>
            <app:PhysicalName>AIRRFL</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AirborneCommandAndControlArea">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Airspace of defined dimensions established specifically for aircraft conducting battlefield command and control.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:e554732c-fa7c-4ac3-913f-aa7ec0f49ac6</app:SemanticID>
            <app:Geometry app:semanticID="MIM:c99353bf-0f8d-4390-8d7b-e1a8ea62b818;MIM:a9198101-9748-4baf-9818-f303f38ed6be">SurfaceVolume;PolygonArea</app:Geometry>
            <app:DisplayName>Airborne command and control area</app:DisplayName>
            <app:PhysicalName>ABC</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AirborneEarlyWarningArea">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Airspace established specifically for airborne platforms conducting early warning.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:79976266-d555-407e-96e2-174f44a60c3a</app:SemanticID>
            <app:Geometry app:semanticID="MIM:c99353bf-0f8d-4390-8d7b-e1a8ea62b818;MIM:a9198101-9748-4baf-9818-f303f38ed6be">SurfaceVolume;PolygonArea</app:Geometry>
            <app:DisplayName>Airborne early warning area</app:DisplayName>
            <app:PhysicalName>AIREWA</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AircraftControlPosition">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">[No definition provided by the external source.]</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:5778cd79-f943-40d9-b3b9-da79d09847a2</app:SemanticID>
            <app:Geometry app:semanticID="MIM:1b27d622-6f58-406d-aceb-2402af195155">AbsolutePoint</app:Geometry>
            <app:DisplayName>Aircraft control position</app:DisplayName>
            <app:PhysicalName>ACCPOS</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AirfieldZone">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The approximate location of the airspace of an airfield.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:6edcb350-14dd-4a28-934a-87d2f1099090</app:SemanticID>
            <app:Geometry app:semanticID="MIM:a9198101-9748-4baf-9818-f303f38ed6be">PolygonArea</app:Geometry>
            <app:DisplayName>Airfield zone</app:DisplayName>
            <app:PhysicalName>ARFLDZ</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Airhead">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A designated area in a hostile or threatened territory which, when seized and held, ensures the continuous air landing of troops and material and provides manoeuvre space for operations. Normally it is the area seized in the assault phase of an airborne or air assault operation. Army--The airhead contains enough drop zones (DZs), landing zones (LZs), and extraction zones (EZs) to ensure mass, interior lines of communication and defence in depth.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:c35d8e3e-0b79-4cc9-a59e-79fac0e61c4c</app:SemanticID>
            <app:Geometry app:semanticID="MIM:a9198101-9748-4baf-9818-f303f38ed6be">PolygonArea</app:Geometry>
            <app:DisplayName>Airhead</app:DisplayName>
            <app:PhysicalName>AIRH</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AirspaceBoundary">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The lateral limits of an airspace control area, airspace control sub-area, high-density airspace control zone or airspace restricted area.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:5537064e-12b7-4823-bffd-dc0f6bd87f35</app:SemanticID>
            <app:Geometry app:semanticID="MIM:f4340665-cee4-4ba0-9e2a-7d5701f46718">Line</app:Geometry>
            <app:DisplayName>Boundary, airspace</app:DisplayName>
            <app:PhysicalName>BOUNDR</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AirspaceControlArea">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Airspace which is laterally defined by boundaries of the area of operations.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:05b7b5c8-785f-42f6-9973-35c5ddf8ba9e</app:SemanticID>
            <app:Geometry app:semanticID="MIM:a9198101-9748-4baf-9818-f303f38ed6be">PolygonArea</app:Geometry>
            <app:DisplayName>Airspace control area</app:DisplayName>
            <app:PhysicalName>AIRCAR</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AirspaceControlSubareaOrSector">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Sub-element of an airspace control area, established to facilitate the control of the overall area.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:6c6e2390-1010-4d43-9c15-b9e6d49af674</app:SemanticID>
            <app:Geometry app:semanticID="MIM:a9198101-9748-4baf-9818-f303f38ed6be">PolygonArea</app:Geometry>
            <app:DisplayName>Airspace control subarea/sector</app:DisplayName>
            <app:PhysicalName>AIRCSA</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AirspaceCoordinationArea">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A three-dimensional block of airspace in a target area, established by the appropriate ground commander, in which friendly aircraft are reasonably safe from friendly surface fire. The Airspace Coordination Area (ACA) may be formal or informal.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:82f5f554-860a-4fee-b304-0c1322ed3f40</app:SemanticID>
            <app:Geometry app:semanticID="MIM:c99353bf-0f8d-4390-8d7b-e1a8ea62b818;MIM:a9198101-9748-4baf-9818-f303f38ed6be">SurfaceVolume;PolygonArea</app:Geometry>
            <app:Geometry app:semanticID="MIM:c99353bf-0f8d-4390-8d7b-e1a8ea62b818;MIM:4bfed303-4f85-44c3-97b0-b9b361889292">SurfaceVolume;RectangleByEdges</app:Geometry>
            <app:Geometry app:semanticID="MIM:c99353bf-0f8d-4390-8d7b-e1a8ea62b818;MIM:a30d557e-f2d3-4e09-b350-d99f04318ac0">SurfaceVolume;Circle</app:Geometry>
            <app:DisplayName>Airspace coordination area</app:DisplayName>
            <app:PhysicalName>AIRCOA</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AlertArea">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Airspace which may contain a high volume of pilot training activities or an unusual type of aerial activity, neither of which is hazardous to aircraft.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:a9ff5f0b-af5e-4eb6-a355-63d95b66c15c</app:SemanticID>
            <app:Geometry app:semanticID="MIM:a9198101-9748-4baf-9818-f303f38ed6be">PolygonArea</app:Geometry>
            <app:DisplayName>Alert area</app:DisplayName>
            <app:PhysicalName>ALRTAR</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AltitudeReservationArea">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Block of altitude, normally medium to high, reserved for aircraft to transit or loiter within the Force Air Coordination Area (FACA) for mission accomplishment.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:4b0d02ea-c672-4b6e-9fb3-26c2c3edb228</app:SemanticID>
            <app:Geometry app:semanticID="MIM:a9198101-9748-4baf-9818-f303f38ed6be">PolygonArea</app:Geometry>
            <app:DisplayName>Altitude reservation area</app:DisplayName>
            <app:PhysicalName>ALTRAR</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AntisubmarineWarfareRotaryWingControlPoint">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">[No definition provided by the external source.]</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:69c2f44e-7546-41e5-a770-91dcea528051</app:SemanticID>
            <app:Geometry app:semanticID="MIM:1b27d622-6f58-406d-aceb-2402af195155">AbsolutePoint</app:Geometry>
            <app:DisplayName>Antisubmarine warfare rotary wing control point</app:DisplayName>
            <app:PhysicalName>ASWRW</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AntisubmarineWarfareStation">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">[No definition provided by the external source.]</xsd:documentation>
          <xsd:appinfo>
