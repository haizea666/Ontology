            <app:DisplayName>Battle position</app:DisplayName>
            <app:PhysicalName>BATPOS</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BattlePositionPreparedButNotOccupied">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A BattlePosition that has been set up but is currently not manned.</xsd:documentation>
