            <app:SemanticID>MIM:989c7c99-15e5-410b-974c-243cabbad48f</app:SemanticID>
            <app:Geometry app:semanticID="MIM:0f665499-bf36-4b91-b752-923e34fe7869">MultiPointCategoryCode::Arrow3To50Points</app:Geometry>
            <app:DisplayName>Axis of supporting attack</app:DisplayName>
            <app:PhysicalName>AXSATK</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BaseCamp">
        <xsd:annotation>
