      <xsd:enumeration value="ArtilleryArea">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">An area assigned to artillery units for terrain management purposes on which the artillery manoeuvres.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:cbc8a91f-a0d5-4cd7-9894-c2e0cb76a64e</app:SemanticID>
