            <app:SemanticID>MIM:ce1b737b-0f00-4009-9c7a-592518dc0352</app:SemanticID>
            <app:Geometry app:semanticID="MIM:a9198101-9748-4baf-9818-f303f38ed6be">PolygonArea</app:Geometry>
            <app:DisplayName>Attack position</app:DisplayName>
            <app:PhysicalName>ATTPOS</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AviationDirectionOfAttack">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A specific direction or route that aviation or the centre of mass of the unit main body of the force will follow.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:2a597e7d-5056-417e-97b2-614bcb21e4bb</app:SemanticID>
