  <xsd:element name="AirControlFeature" type="controlfeature:AirControlFeatureType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">An Air Control Feature for which no further information is given other than its category.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:7db9ee09-6e77-4433-a176-fbfa7bc14b36</app:SemanticID>
        <app:Geometry app:semanticID="MIM:1b27d622-6f58-406d-aceb-2402af195155" app:inheritedFrom="MIM:98a630cb-1c93-4eef-ab3a-c95cd4c7c74f">AbsolutePoint</app:Geometry>
