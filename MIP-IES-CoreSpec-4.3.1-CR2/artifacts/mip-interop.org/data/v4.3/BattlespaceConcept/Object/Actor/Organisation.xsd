    </xsd:annotation>
    <xsd:restriction base="primitives:CodeValueType">
      <xsd:enumeration value="MOPPReady">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Mask is carried. First set individual protection equipment is available within 2 hours, second set individual protection equipment is available within 6 hours.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:4494f1d5-db64-4d64-abe0-330d49a4bbf0</app:SemanticID>
