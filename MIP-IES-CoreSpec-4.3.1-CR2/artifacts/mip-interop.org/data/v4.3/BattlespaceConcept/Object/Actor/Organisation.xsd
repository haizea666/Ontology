            <app:PhysicalName>MOPPRY</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="MOPP0">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Mask is carried. Individual protection equipment is immediately available.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:c4b0e425-d7d5-4ad1-8b57-4e62ba33d090</app:SemanticID>
            <app:DisplayName>MOPP 0</app:DisplayName>
            <app:PhysicalName>MOPP0</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="MOPP1">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Mask is carried. Suit worn, boots and gloves are carried.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:6349b8f3-52ca-4984-9910-e5d41f95d013</app:SemanticID>
            <app:DisplayName>MOPP 1</app:DisplayName>
            <app:PhysicalName>MOPP1</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="MOPP2">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Mask is carried. Suit and boots worn, gloves are carried.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:1f97408a-dbfc-4130-b0e0-239e243a9daa</app:SemanticID>
            <app:DisplayName>MOPP 2</app:DisplayName>
            <app:PhysicalName>MOPP2</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="MOPP3">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Mask is carried. Suit, boots and gloves are worn.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:90ff1618-4552-4cbd-aa69-6f6d195ed76d</app:SemanticID>
            <app:DisplayName>MOPP 3</app:DisplayName>
            <app:PhysicalName>MOPP3</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
