  <xsd:simpleType name="OrganisationCommandAndControlAssociationCategoryCodeType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The type of command and control association between two Organisations.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:f19c48f1-d371-407b-8c6e-781064e1da3b</app:SemanticID>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="primitives:CodeValueType">
      <xsd:enumeration value="HasAdministrativeControlOf">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Direction or exercise of authority over subordinate or other organisations in respect to administrative matters such as personnel management, supply, services, and other matters not included in the operational missions of the subordinate or other organisations.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:b30c4b6f-4d79-4985-a57a-edcebc0828d9</app:SemanticID>
            <app:DisplayName>Has administrative control of</app:DisplayName>
            <app:PhysicalName>ADMCON</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="HasAttached">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The placement of units in an organisation where such placement is relatively temporary.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:323118a8-2160-4d7a-8265-5454b2bcd939</app:SemanticID>
            <app:DisplayName>Has attached</app:DisplayName>
            <app:PhysicalName>ATTACH</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="HasCoordinationAuthorityOver">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The authority granted to a commander or individual assigned responsibility for coordinating specific functions or activities involving forces of two or more countries or commands, or two or more services or two or more forces of the same service.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:9fbd2115-932c-44f5-9613-67a091bbc135</app:SemanticID>
            <app:DisplayName>Has coordination authority over</app:DisplayName>
            <app:PhysicalName>COAUTH</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="HasDetached">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The subject Organisation has the object Organisation separated from its main organisation for duty elsewhere.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:827aa4c4-0b60-4882-976a-1fd987094fe1</app:SemanticID>
            <app:DisplayName>Has detached</app:DisplayName>
            <app:PhysicalName>DETACH</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="HasFullCommandOf">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The military authority and responsibility of a superior officer to issue orders to subordinates.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:07c34f55-489b-44c0-8c24-ae8980fd6d9f</app:SemanticID>
            <app:DisplayName>Has full command of</app:DisplayName>
            <app:PhysicalName>COMD</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="HasLogisticalControlOf">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Logistic Control (LOGCON) is that authority granted to a Commander over assigned logistics units and organizations in the JOA, including National Support Elements (NSE) that empowers him to synchronize, prioritize, and integrate their logistics functions and activities to accomplish the joint mission.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:d08eb9eb-e14a-4453-af38-4d57b079b7ab</app:SemanticID>
            <app:DisplayName>Has logistical control of</app:DisplayName>
            <app:PhysicalName>LOGCON</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="HasOperationalCommandOf">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The authority granted to a commander to assign missions or tasks to subordinate commanders, to deploy units, to reassign forces, and to retain or delegate operational and/or tactical control as may be deemed necessary.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:65cbc72d-ce51-4ec5-83e9-7227c65af080</app:SemanticID>
            <app:DisplayName>Has operational command of</app:DisplayName>
            <app:PhysicalName>OPCOMD</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="HasOperationalControlOf">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The authority delegated to a commander to direct forces assigned so that the commander may accomplish specific missions or tasks which are usually limited by function, time, or location; to deploy units concerned, and to retain or assign tactical control of those units. It does not include authority to assign separate employment of components of the units concerned.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:d3f91240-24cd-4b14-b7f2-a0e8ee3d3ae7</app:SemanticID>
            <app:DisplayName>Has operational control of</app:DisplayName>
            <app:PhysicalName>OPCON</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="HasTacticalCommandOf">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The authority delegated to a commander to assign tasks to forces under his command for the accomplishment of the mission assigned by higher authority.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:61dae9f0-c10a-483d-9fbc-dc730098d9f8</app:SemanticID>
            <app:DisplayName>Has tactical command of</app:DisplayName>
            <app:PhysicalName>TACCOM</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
