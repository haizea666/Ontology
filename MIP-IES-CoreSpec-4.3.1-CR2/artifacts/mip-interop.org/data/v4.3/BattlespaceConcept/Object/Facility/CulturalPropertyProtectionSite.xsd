    <xsd:annotation>
      <xsd:documentation xml:lang="en">The type of threat to a CulturalPropertyProtectionSite.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:5e49aea8-3300-4f11-8fe9-fe147337732d</app:SemanticID>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="primitives:CodeValueType">
      <xsd:enumeration value="ChemicalLeaching">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The site is threatened by a chemical leaching.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:ea030374-608c-43cd-8d2f-4e58af9c7b3a</app:SemanticID>
            <app:Remarks>Common sources: construction (roads, foundations, etc.), petrol / oil.</app:Remarks>
            <app:DisplayName>Chemical leaching</app:DisplayName>
            <app:PhysicalName>CHLEAC</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DiggingOrExcavation">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The site is threatened by a digging or excavation.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:607e210a-d3de-42ce-9721-f4e0b239acc5</app:SemanticID>
            <app:Remarks>Common sources: bulldozing, construction, looting, mining, drilling, earthwork creation, tunneling.</app:Remarks>
            <app:DisplayName>Digging / excavation</app:DisplayName>
            <app:PhysicalName>DIGEXC</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="KineticForce">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The site is threatened by kinetic forces.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:6dea4f7c-63e8-41b8-9e2b-b947c1971710</app:SemanticID>
            <app:Remarks>Common sources: Explosives, landmines, heavy weaponry.</app:Remarks>
            <app:DisplayName>Kinetic force</app:DisplayName>
            <app:PhysicalName>KINFRC</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="LootingOfMaterials">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The site is threatened to have its materials looted.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:a97a48fc-022d-4a34-bf2f-eae64a1c6a07</app:SemanticID>
            <app:Remarks>Common sources: stone robbing, looting of fixtures and fittings.</app:Remarks>
            <app:DisplayName>Looting (of materials)</app:DisplayName>
            <app:PhysicalName>LOOTMA</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="LootingOfObjects">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The site is threatened to have its objects looted.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:e0b4e369-72c7-4bb9-8cfb-0d6eb8b2175f</app:SemanticID>
            <app:Remarks>Common sources: pillage / spoils of war, criminal activity.</app:Remarks>
            <app:DisplayName>Looting (of objects)</app:DisplayName>
            <app:PhysicalName>LOOTOB</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Vandalism">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The site is threatened by vandalism.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:d5c33630-ae03-45a8-a630-c6890d36091c</app:SemanticID>
            <app:Remarks>Common sources: people.</app:Remarks>
            <app:DisplayName>Vandalism</app:DisplayName>
            <app:PhysicalName>VANDAL</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Vibrations">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The site is threatened by vibrations.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:85bb2195-1166-4791-9677-539e7979d36c</app:SemanticID>
            <app:Remarks>Common source: heavy machinery, heavy vehicles.</app:Remarks>
            <app:DisplayName>Vibrations</app:DisplayName>
            <app:PhysicalName>VIBRAT</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="WearOrAttritionOrErosion">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The site is threatened by wear, attrition or erosion.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:d50a9009-de8c-4c4e-979b-d4d43c28c84b</app:SemanticID>
            <app:Remarks>Common sources : inappropriate reuse of cultural locations.</app:Remarks>
            <app:DisplayName>Wear / attrition / erosion</app:DisplayName>
            <app:PhysicalName>WEAR</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="CulturalPropertyProtectionSiteType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">A site under Cultural Property Protection (CPP).</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:9d37c42d-06ab-42ec-af2f-b857beac8cc5</app:SemanticID>
        <app:Geometry app:semanticID="MIM:1b27d622-6f58-406d-aceb-2402af195155">AbsolutePoint</app:Geometry>
        <app:DisplayName>Cultural property protection site</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="facility:FacilityType">
        <xsd:sequence>
          <xsd:element ref="culturalpropertyprotectionsite:CulturalPropertyProtectionSiteIsUnderWaterIndicator" minOccurs="0" />
          <xsd:element ref="culturalpropertyprotectionsite:CulturalPropertyProtectionSiteJctdsCode" minOccurs="0" maxOccurs="unbounded" />
          <xsd:element ref="culturalpropertyprotectionsite:CulturalPropertyProtectionSiteLegalRightsCode" minOccurs="0" />
          <xsd:element ref="culturalpropertyprotectionsite:CulturalPropertyProtectionSiteSignificanceCode" minOccurs="0" />
          <xsd:element ref="culturalpropertyprotectionsite:CulturalPropertyProtectionSiteSignificanceDescriptionText" minOccurs="0" />
          <xsd:element ref="culturalpropertyprotectionsite:CulturalPropertyProtectionSiteThreatCode" minOccurs="0" />
          <xsd:element ref="culturalpropertyprotectionsite:CulturalPropertyProtectionSiteTypeCode" minOccurs="0" />
          <xsd:element ref="culturalpropertyprotectionsite:CulturalPropertyProtectionSiteVulnerabilityCode" minOccurs="0" />
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="CulturalPropertyProtectionSiteTypeCode" type="culturalpropertyprotectionsite:CulturalPropertyProtectionSiteTypeCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The type of the CulturalPropertyProtectionSite, whether it is natural, cultural or mixed.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:8d382035-ffd9-468c-85e0-f9a3b28a0dae</app:SemanticID>
        <app:ShortName>typeCode</app:ShortName>
        <app:MIMStereotype>code</app:MIMStereotype>
        <app:MIMStereotype>type</app:MIMStereotype>
        <app:DisplayName>type</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="CulturalPropertyProtectionSiteTypeCodeType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The type of a CulturalPropertyProtectionSite, whether it is natural, cultural or mixed.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:14e79953-6f40-4178-8799-fe436a6ee27e</app:SemanticID>
        <app:MIMStereotype>complete</app:MIMStereotype>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="primitives:CodeValueType">
      <xsd:enumeration value="Cultural">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A site that is recognised in the 1954 Hague Convention for the Protection of Cultural Property in the Event of Armed Conflict.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:bc8b570a-5df0-4818-87b1-ad1ebc59b9aa</app:SemanticID>
            <app:Remarks>The Convention gives non-exhaustive examples of CP including
a. Movable or immovable property of great importance to the cultural heritage of every people, such as monuments of architecture, art or history, whether religious or secular; archaeological sites; groups of buildings which, as a whole, are of historical or artistic interest; works of art; manuscripts, books and other objects of artistic, historical or archaeological interest; as well as scientific collections and important collections of books or archives or of reproductions of the property defined above;
b. buildings whose main and effective purpose is to preserve or exhibit the movable cultural property defined in sub-paragraph (a) such as museums, large libraries and depositories of archives, and refuges intended to shelter, in the event of armed conflict, the movable cultural property defined in sub-paragraph (a);
c. Centres containing a large amount of cultural property as defined in subparagraphs (a) and (b), to be known as "centres containing monuments".
Physical CP covered by the convention may be underwater, and may have intangible anthropological elements, detailed in the site description. However, these must be linked to a bounded geographic entity (physical or natural).</app:Remarks>
            <app:DisplayName>Cultural</app:DisplayName>
            <app:PhysicalName>CULTRL</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Mixed">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The site contains both cultural and natural elements.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:869d0aff-0d95-408f-9ac2-1a3b425fd295</app:SemanticID>
            <app:DisplayName>Mixed</app:DisplayName>
            <app:PhysicalName>MIXED</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Natural">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A site that is recognized due to its ecological value.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:b2dc5a07-b718-4109-9d8b-249295a0b230</app:SemanticID>
            <app:Remarks>Natural resources may include but are not limited to: (1) Species and/or habitats of endangered, vulnerable, and/or threatened fauna and flora, (2) Protected geographical areas and the flora/fauna within those areas; protected areas include, but are not limited to, trans-national/national/regional parks, wildlife/forest/marine reserves, and sanctuaries; (3) Geological/physiographical formations; and (4) Challenged environments.
Natural resources may have intangible anthropological elements, detailed in the site description. However, these must be linked to a bounded geographic entity (physical or natural).</app:Remarks>
            <app:DisplayName>Natural</app:DisplayName>
            <app:PhysicalName>NATURL</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:element name="CulturalPropertyProtectionSiteVulnerabilityCode" type="culturalpropertyprotectionsite:CulturalPropertyProtectionSiteVulnerabilityCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The natural or man-made events/risks that may result in damage to the CPP site.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:775d1569-e37a-478e-b843-217b2f756350</app:SemanticID>
        <app:ShortName>vulnerabilityCode</app:ShortName>
        <app:Remarks>These may be immediate and likely to have an effect within 1-2 years. Many Japanese structures, for example, are particularly vulnerable to fire. However, whilst mud-brick is vulnerable to rain, it will not typically have an immediate effect in the short-term.</app:Remarks>
        <app:MIMStereotype>code</app:MIMStereotype>
        <app:MIMStereotype>instance</app:MIMStereotype>
        <app:DisplayName>vulnerability</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="CulturalPropertyProtectionSiteVulnerabilityCodeType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The natural or man-made events/risks that may result in damage to a CPP site.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:33ac25ca-51f1-4c18-bfa5-8a7c3f56af2e</app:SemanticID>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="primitives:CodeValueType">
      <xsd:enumeration value="Criminals">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Illegal activity carried out by individuals or groups that result in disappearance, destruction, disfiguration, etc of the CPP site or objects.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:3b97fb9b-959f-4016-8d46-1c5422a0087e</app:SemanticID>
            <app:Remarks>The motivation for such acts can be political, ideological, economic motivation, etc.</app:Remarks>
            <app:DisplayName>Criminals</app:DisplayName>
            <app:PhysicalName>CRIMNL</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Dissociation">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The susceptibility of a CPP site due to loss of information about heritage assets, (temporary) loss or inability to access heritage items, etc as a result of the lack of inventory, poor documentation or identification, misplacing objects, hardware and software obsolescence, staff retirement, etc.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:3d0bf899-aa3e-4f1b-91df-3000acd01380</app:SemanticID>
            <app:DisplayName>Dissociation</app:DisplayName>
            <app:PhysicalName>DISSOC</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Fire">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The susceptibility to the total or partial burning, collapse or deformation by heating, soot deposition, etc of the CPP site.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:28ec6c1e-f7b3-43f4-bbfa-504d46c3ddca</app:SemanticID>
            <app:Remarks>This may be caused by lightning, forest fires, gas leaks, fireworks, faulty electrical installations or equipment, smoking, candles, arson, construction and renovation works, etc.</app:Remarks>
            <app:DisplayName>Fire</app:DisplayName>
            <app:PhysicalName>FIRE</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="PestsOrLossOfEnvironmentControl">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The susceptibility to the invasion of a CPP site by local fauna (insects, rodents, birds, bats, etc). Sources of food and nesting materials attract pests resulting in staining, perforation, weakening, loss of parts, etc of the site.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:228a6aa8-6311-4deb-8ec5-8381ac1e02df</app:SemanticID>
            <app:Remarks>The loss of site climactic control resulting in colour fading (primary effect of light); yellowing, weakening and disintegration (primary effects of UV), due to Sun, electrical light sources (lamps);  or the faster deterioration by chemical reactions, deformation, dehydration, embrittlement, softening, etc due to incorrect temperature due to changes in local climate, sunlight, incandescent lamps, heaters, etc.
Damage caused by incorrect relative humidity which can cause damage to the site for example deformation, cracking, flaking, delamination, weakening, corrosion, mould growth, staining, etc as a result of changes in the local climate, ground water, inadequate air conditioning, micro-climates, etc.</app:Remarks>
            <app:DisplayName>Pests / loss of environment control</app:DisplayName>
            <app:PhysicalName>PSTENC</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="PhysicalForce">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">These are related to damage that may be caused to a CPP site due to; incorrect handling, storage, transportation; collisions, wind erosion, excavations, construction works, armed conflict, earthquakes, traffic, overload, etc.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:121e07dd-fe7f-4103-8c3f-cf8fcee19f78</app:SemanticID>
            <app:Remarks>These may result in collapse, deformation, breakage, abrasion, wear, tearing, etc of the CPPsite.</app:Remarks>
            <app:DisplayName>Physical force</app:DisplayName>
            <app:PhysicalName>PHSFRC</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Pollutants">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The susceptibility of a CPP site to suffer discoloration, weakening, staining, darkening, erosion, corrosion, etc as a direct result of the following sources, industrial processes, vehicles, construction and renovation works, storage and display materials that emit gases, visitors, restoration materials that contaminate the object, etc.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:ef619a05-55b2-496e-83a6-fbe8a70fa80d</app:SemanticID>
            <app:DisplayName>Pollutants</app:DisplayName>
            <app:PhysicalName>PLLTNT</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Water">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The susceptibility of the CPP site to staining, weakening, deformation, dissolution, corrosion, weathering, salt efflorescence, biological growth, etc.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:16a82d43-11e4-4350-8746-d4ef99430545</app:SemanticID>
            <app:Remarks>This may be as a result of Tsunami, flooded rivers, rain, ground water, water pipes, cleaning procedures, firefighting, etc.</app:Remarks>
            <app:DisplayName>Water</app:DisplayName>
            <app:PhysicalName>WATER</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:element name="HumanRemainsSite" type="culturalpropertyprotectionsite:HumanRemainsSiteType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">A site which contains the human remains which may be old or modern.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:7369160e-0670-4ddd-b067-fecf01ee3fca</app:SemanticID>
        <app:Remarks>Human internments can include burials under floors, in walls, and other locations, as well as more obvious cemeteries. They may be scattered across the site, and found in multiple locations.</app:Remarks>
        <app:Geometry app:semanticID="MIM:1b27d622-6f58-406d-aceb-2402af195155" app:inheritedFrom="MIM:9d37c42d-06ab-42ec-af2f-b857beac8cc5">AbsolutePoint</app:Geometry>
        <app:DisplayName>Human remains site</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="HumanRemainsSiteHasArchaeologicalBurialsIndicator" type="culturalpropertyprotectionsite:HumanRemainsSiteHasArchaeologicalBurialsIndicatorType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">Whether the site contains archaeological burials (true) or not (false).</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:5f7c1401-e735-415b-a1eb-538b8de8e99e</app:SemanticID>
        <app:ShortName>hasArchaeologicalBurialsIndicator</app:ShortName>
        <app:MIMStereotype>indicator</app:MIMStereotype>
        <app:MIMStereotype>instance</app:MIMStereotype>
        <app:DisplayName>has archaeological burials</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="HumanRemainsSiteHasArchaeologicalBurialsIndicatorType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">Whether the site contains archaeological burials (true) or not (false).</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:5f7c1401-e735-415b-a1eb-538b8de8e99e</app:SemanticID>
        <app:ShortName>hasArchaeologicalBurialsIndicator</app:ShortName>
        <app:MIMStereotype>indicator</app:MIMStereotype>
        <app:MIMStereotype>instance</app:MIMStereotype>
        <app:DisplayName>has archaeological burials</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="primitives:BooleanType" />
  </xsd:simpleType>
  <xsd:element name="HumanRemainsSiteHasModernBurialsIndicator" type="culturalpropertyprotectionsite:HumanRemainsSiteHasModernBurialsIndicatorType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">Whether the site contains modern burials (true) or not (false).</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:f3f344cb-10c2-4663-8aa3-8a2c392023c0</app:SemanticID>
        <app:ShortName>hasModernBurialsIndicator</app:ShortName>
        <app:Remarks>Modern is not dictated by age, but by association with an existing community. For example, a 12th century AD cemetery would be archaeological, but a shrine associated with a companion of the prophet Mohammed would be modern.</app:Remarks>
        <app:MIMStereotype>indicator</app:MIMStereotype>
        <app:MIMStereotype>instance</app:MIMStereotype>
        <app:DisplayName>has modern burials</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="HumanRemainsSiteHasModernBurialsIndicatorType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">Whether the site contains modern burials (true) or not (false).</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:f3f344cb-10c2-4663-8aa3-8a2c392023c0</app:SemanticID>
        <app:ShortName>hasModernBurialsIndicator</app:ShortName>
        <app:Remarks>Modern is not dictated by age, but by association with an existing community. For example, a 12th century AD cemetery would be archaeological, but a shrine associated with a companion of the prophet Mohammed would be modern.</app:Remarks>
        <app:MIMStereotype>indicator</app:MIMStereotype>
        <app:MIMStereotype>instance</app:MIMStereotype>
        <app:DisplayName>has modern burials</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="primitives:BooleanType" />
  </xsd:simpleType>
  <xsd:element name="HumanRemainsSiteHasMoreThan5GravesIndicator" type="culturalpropertyprotectionsite:HumanRemainsSiteHasMoreThan5GravesIndicatorType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">Whether the site has more than 5 graves (true) or not (false).</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:e61a8194-9f48-4f2d-a07d-2e7d9c99e3ed</app:SemanticID>
        <app:ShortName>hasMoreThan5GravesIndicator</app:ShortName>
        <app:MIMStereotype>indicator</app:MIMStereotype>
        <app:MIMStereotype>instance</app:MIMStereotype>
        <app:DisplayName>has more than 5 graves</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="HumanRemainsSiteHasMoreThan5GravesIndicatorType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">Whether the site has more than 5 graves (true) or not (false).</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:e61a8194-9f48-4f2d-a07d-2e7d9c99e3ed</app:SemanticID>
        <app:ShortName>hasMoreThan5GravesIndicator</app:ShortName>
        <app:MIMStereotype>indicator</app:MIMStereotype>
        <app:MIMStereotype>instance</app:MIMStereotype>
        <app:DisplayName>has more than 5 graves</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="primitives:BooleanType" />
  </xsd:simpleType>
  <xsd:complexType name="HumanRemainsSiteType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">A site which contains the human remains which may be old or modern.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:7369160e-0670-4ddd-b067-fecf01ee3fca</app:SemanticID>
        <app:Remarks>Human internments can include burials under floors, in walls, and other locations, as well as more obvious cemeteries. They may be scattered across the site, and found in multiple locations.</app:Remarks>
        <app:Geometry app:semanticID="MIM:1b27d622-6f58-406d-aceb-2402af195155" app:inheritedFrom="MIM:9d37c42d-06ab-42ec-af2f-b857beac8cc5">AbsolutePoint</app:Geometry>
        <app:DisplayName>Human remains site</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="culturalpropertyprotectionsite:CulturalPropertyProtectionSiteType">
        <xsd:sequence>
          <xsd:element ref="culturalpropertyprotectionsite:HumanRemainsSiteHasArchaeologicalBurialsIndicator" minOccurs="0" />
          <xsd:element ref="culturalpropertyprotectionsite:HumanRemainsSiteHasModernBurialsIndicator" minOccurs="0" />
          <xsd:element ref="culturalpropertyprotectionsite:HumanRemainsSiteHasMoreThan5GravesIndicator" minOccurs="0" />
        </xsd:sequence>
      </xsd:extension>
