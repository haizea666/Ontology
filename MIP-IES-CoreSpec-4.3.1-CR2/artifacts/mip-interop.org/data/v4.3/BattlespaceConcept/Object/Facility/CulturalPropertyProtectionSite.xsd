  <xsd:simpleType name="CulturalPropertyBuildingOrStructureCategoryCodeType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The class of CulturalPropertyBuildingOrStructure.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:b27f6cf4-ab7e-4b22-967a-45041cb9bcb1</app:SemanticID>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="primitives:CodeValueType">
      <xsd:enumeration value="GroupOfBuildingsOrSitesOrFeaturesOrCentresContainingMonuments">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A group of buildings, sites, features or centres containing monuments of cultural interest that are placed under Cultural Property Protection.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:b430e995-5b92-484e-aa0e-ee76a14b191b</app:SemanticID>
            <app:Remarks>For example, a historic city centre, or World Heritage site that contains a group of features. If possible, individual features should give their own linked record.</app:Remarks>
            <app:Geometry app:semanticID="MIM:1b27d622-6f58-406d-aceb-2402af195155" app:inheritedFrom="MIM:9d37c42d-06ab-42ec-af2f-b857beac8cc5">AbsolutePoint</app:Geometry>
            <app:DisplayName>Cultural property protection monuments enclosure</app:DisplayName>
            <app:PhysicalName>MONENC</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ObjectCollectionPermanentStorageBuilding">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Buildings which are normally the permanent location of a collections; these may be museums or private buildings. If the building is also historic in its own right, then that takes primacy, and is the category that should be entered. This field indicates buildings which are only significant due to their storage facilities.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:09f21a27-7308-463a-96e9-9f00130bf377</app:SemanticID>
            <app:Geometry app:semanticID="MIM:1b27d622-6f58-406d-aceb-2402af195155" app:inheritedFrom="MIM:9d37c42d-06ab-42ec-af2f-b857beac8cc5">AbsolutePoint</app:Geometry>
            <app:DisplayName>Object collection permanent storage building</app:DisplayName>
            <app:PhysicalName>PRMBLD</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ObjectCollectionRefugeBuilding">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A purpose built refuge for collections, or a temporary storage location for collections in the event the collection has been evacuated due to natural disaster or armed conflict (as recommended in the 1954 Hague Convention). The collection may not often be present at the location, and may not be present at the time of entry.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:bc59b2f2-c09d-4d73-98af-3d37d6342ccd</app:SemanticID>
            <app:Geometry app:semanticID="MIM:1b27d622-6f58-406d-aceb-2402af195155" app:inheritedFrom="MIM:9d37c42d-06ab-42ec-af2f-b857beac8cc5">AbsolutePoint</app:Geometry>
            <app:DisplayName>Object collection refuge building</app:DisplayName>
            <app:PhysicalName>REFBLD</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ReligiousBuildingOrStructure">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A religious building or structure (Church, Chapel, cathedral, mosque, madrasa, shrine, synagogue, etc).</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:2e7dc564-be1a-498f-8654-17de07b14dcf</app:SemanticID>
            <app:Geometry app:semanticID="MIM:1b27d622-6f58-406d-aceb-2402af195155" app:inheritedFrom="MIM:9d37c42d-06ab-42ec-af2f-b857beac8cc5">AbsolutePoint</app:Geometry>
            <app:DisplayName>Religious building or structure</app:DisplayName>
            <app:PhysicalName>RELBLD</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ReligiousComplex">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Religious complex of buildings and structures, such as a monastic complex.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:4b10086f-db63-48e0-af99-f096d78a6bc7</app:SemanticID>
            <app:Geometry app:semanticID="MIM:1b27d622-6f58-406d-aceb-2402af195155" app:inheritedFrom="MIM:9d37c42d-06ab-42ec-af2f-b857beac8cc5">AbsolutePoint</app:Geometry>
            <app:DisplayName>Religious complex</app:DisplayName>
            <app:PhysicalName>RELCMP</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="SecularBuildingOrStructure">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A historic or otherwise significant building or structure, such as bridge, windmill, and so on.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:f2a60592-8a15-4333-b94f-b368e96f8c55</app:SemanticID>
            <app:Geometry app:semanticID="MIM:1b27d622-6f58-406d-aceb-2402af195155" app:inheritedFrom="MIM:9d37c42d-06ab-42ec-af2f-b857beac8cc5">AbsolutePoint</app:Geometry>
            <app:DisplayName>Secular building or structure</app:DisplayName>
            <app:PhysicalName>SECBLD</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="SecularComplex">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Complex of buildings and structures, such as a listed terrace of old houses, or an old airfield and buildings.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:149a794c-fd71-46d7-ab16-bf1b8a7b007c</app:SemanticID>
            <app:Geometry app:semanticID="MIM:1b27d622-6f58-406d-aceb-2402af195155" app:inheritedFrom="MIM:9d37c42d-06ab-42ec-af2f-b857beac8cc5">AbsolutePoint</app:Geometry>
            <app:DisplayName>Secular complex</app:DisplayName>
            <app:PhysicalName>SECCMP</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
