      <xsd:documentation xml:lang="en">A place in which evidence of past activity is preserved, and which has been, or may be, investigated using the discipline of archaeology and represents a part of the archaeological record.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:f6a77b06-3250-4310-83ba-799e6dcbf24e</app:SemanticID>
        <app:Geometry app:semanticID="MIM:1b27d622-6f58-406d-aceb-2402af195155" app:inheritedFrom="MIM:9d37c42d-06ab-42ec-af2f-b857beac8cc5">AbsolutePoint</app:Geometry>
        <app:DisplayName>Archaeological site</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="culturalpropertyprotectionsite:CulturalPropertyProtectionSiteType">
        <xsd:sequence>
          <xsd:element ref="culturalpropertyprotectionsite:ArchaeologicalSiteCategoryCode" />
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="CulturalPropertyBuildingOrStructure" type="culturalpropertyprotectionsite:CulturalPropertyBuildingOrStructureType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">A building or structure representing or containing a cultural property.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:386a730b-ed35-4f95-84d3-d52350284bdd</app:SemanticID>
        <app:Geometry app:semanticID="MIM:1b27d622-6f58-406d-aceb-2402af195155" app:inheritedFrom="MIM:9d37c42d-06ab-42ec-af2f-b857beac8cc5">AbsolutePoint</app:Geometry>
