  <xsd:element name="ArchaeologicalSite" type="culturalpropertyprotectionsite:ArchaeologicalSiteType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">A place in which evidence of past activity is preserved, and which has been, or may be, investigated using the discipline of archaeology and represents a part of the archaeological record.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:f6a77b06-3250-4310-83ba-799e6dcbf24e</app:SemanticID>
        <app:Geometry app:semanticID="MIM:1b27d622-6f58-406d-aceb-2402af195155" app:inheritedFrom="MIM:9d37c42d-06ab-42ec-af2f-b857beac8cc5">AbsolutePoint</app:Geometry>
        <app:DisplayName>Archaeological site</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ArchaeologicalSiteCategoryCode" type="culturalpropertyprotectionsite:ArchaeologicalSiteCategoryCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The class of ArchaeologicalSite.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:1367272e-3a53-46f8-a8f9-dc7b05130939</app:SemanticID>
        <app:ShortName>categoryCode</app:ShortName>
        <app:ValueOrAmplification>1</app:ValueOrAmplification>
        <app:MIMStereotype>code</app:MIMStereotype>
        <app:MIMStereotype>discriminator</app:MIMStereotype>
        <app:MIMStereotype>type</app:MIMStereotype>
        <app:DisplayName>category</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="ArchaeologicalSiteCategoryCodeType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The class of ArchaeologicalSite.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:6f82a7e8-269b-4e80-bbb0-3a5b747a0fb1</app:SemanticID>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="primitives:CodeValueType">
      <xsd:enumeration value="ArchaeologicalSiteBuried">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">An archaeological site primarily composed of buried remains that are not visible from the ground.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:9d4b5206-3843-4331-81cf-96e80f06166f</app:SemanticID>
            <app:Geometry app:semanticID="MIM:1b27d622-6f58-406d-aceb-2402af195155" app:inheritedFrom="MIM:9d37c42d-06ab-42ec-af2f-b857beac8cc5">AbsolutePoint</app:Geometry>
            <app:DisplayName>Archaeological site (buried)</app:DisplayName>
            <app:PhysicalName>ARCBUR</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ArchaeologicalSiteMixedFeatures">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">An archaeological site composed of mixture of buried, standing and/or surface features.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:785531f4-d18c-45eb-9a5c-2e621d280c60</app:SemanticID>
            <app:Geometry app:semanticID="MIM:1b27d622-6f58-406d-aceb-2402af195155" app:inheritedFrom="MIM:9d37c42d-06ab-42ec-af2f-b857beac8cc5">AbsolutePoint</app:Geometry>
            <app:DisplayName>Archaeological site (mixed features)</app:DisplayName>
            <app:PhysicalName>ARCMIX</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ArchaeologicalSiteStandingRemainsOrRuins">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">An archaeological site composed standing remains / ruins. This would include, for example, ruined buildings such as ancient temples and churches.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:ced18b04-bba5-4bbe-b083-661d047745b8</app:SemanticID>
            <app:Geometry app:semanticID="MIM:1b27d622-6f58-406d-aceb-2402af195155" app:inheritedFrom="MIM:9d37c42d-06ab-42ec-af2f-b857beac8cc5">AbsolutePoint</app:Geometry>
            <app:DisplayName>Archaeological site (standing remains/ruins)</app:DisplayName>
            <app:PhysicalName>ARCSTD</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ArchaeologicalSiteSurfaceFeatures">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">An archaeological site composed primarily of surface features, such as archaeoloigical soils and cropmarks, earthworks, roads, ruined cairns, and other features that have little or no height remaining. If it is unknown whether the surface feature links to significant buried remains, then surface feature should be chosen.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:7a6e3eaa-35c5-4c77-932e-3e7dd36e384a</app:SemanticID>
            <app:Geometry app:semanticID="MIM:1b27d622-6f58-406d-aceb-2402af195155" app:inheritedFrom="MIM:9d37c42d-06ab-42ec-af2f-b857beac8cc5">AbsolutePoint</app:Geometry>
            <app:DisplayName>Archaeological site (surface features)</app:DisplayName>
            <app:PhysicalName>ARCSUR</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="ArchaeologicalSiteType">
