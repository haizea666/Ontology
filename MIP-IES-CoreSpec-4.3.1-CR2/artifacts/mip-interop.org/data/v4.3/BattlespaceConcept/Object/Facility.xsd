  <xsd:element name="ApproachDirectionVisualGlideSlopeLightingCode" type="facility:ApproachDirectionVisualGlideSlopeLightingCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The type of visual aid assisting pilots landing an aircraft on the Runway, by displaying if the aircraft is on the correct path of descent.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:c6a79d42-a591-4093-8e77-6396cdd83d54</app:SemanticID>
        <app:ShortName>visualGlideSlopeLightingCode</app:ShortName>
