            <app:SemanticID>MIM:b73f4613-dd6c-4747-a91c-18619269aab5</app:SemanticID>
            <app:DisplayName>Underwater sled</app:DisplayName>
            <app:PhysicalName>SWUNDS</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
