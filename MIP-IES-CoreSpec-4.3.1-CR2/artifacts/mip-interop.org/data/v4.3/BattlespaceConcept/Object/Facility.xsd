  </xsd:element>
  <xsd:simpleType name="MaritimeMinefieldBottomNaturalCamouflageCodeType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The description of the ground of a lake, river, or ocean, or other body of water with respect to the ability to hide mines on the bottom at a MaritimeMinefield.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:e0afbd1e-3d32-4398-812e-730199892e9e</app:SemanticID>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="primitives:CodeValueType">
      <xsd:enumeration value="BottomCoveredInSeaweed">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Mines are likely to be hidden by seaweed, mine hunting possible with difficulty.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:0a569e38-51eb-417c-9fbf-4323f1b232e9</app:SemanticID>
            <app:DisplayName>Bottom covered in seaweed</app:DisplayName>
            <app:PhysicalName>DV</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BottomHasDeepHollowsOrCrevicesOrCliffs">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Mines are likely to be hidden completely in deep hollows or crevices or by cliffs, mine hunting difficult.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:483d4458-e142-4e81-844a-8415d667584c</app:SemanticID>
            <app:DisplayName>Bottom has deep hollows or crevices or cliffs</app:DisplayName>
            <app:PhysicalName>DH</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="IrregularBottom">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Mines are likely to be hidden by irregularity of the bottom, mine hunting difficult.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:8b6b343d-68fa-4e42-abc7-458632aa9acf</app:SemanticID>
            <app:DisplayName>Irregular bottom</app:DisplayName>
            <app:PhysicalName>DR</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="RatherStableAndSmoothButUnevenBottom">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Holes, bumps, ridges and folds up to 30 cm and/or mine burial possible but never exceeding 30 cm.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:98aa9142-5290-42b7-9b2e-e6a8e750b20c</app:SemanticID>
            <app:DisplayName>Rather stable and smooth but uneven bottom</app:DisplayName>
            <app:PhysicalName>B</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="RoughBottom">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Holes, bumps, ridges and folds up to 30 cm and/or a lot of seaweed. Mine burial likely (exceeding 30 cm) but never complete.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:7568dea6-a8b9-4b24-9bfe-f46dd5f36cbc</app:SemanticID>
            <app:DisplayName>Rough bottom</app:DisplayName>
            <app:PhysicalName>C</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="SoftBottom">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Mines may be buried permanently or break surface from time to time.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:d6dd6907-7f84-48a7-80b6-f49414daa5bc</app:SemanticID>
            <app:DisplayName>Soft bottom</app:DisplayName>
            <app:PhysicalName>DB</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="StableAndSmoothFlatBottom">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Ripples less than 15 cm deep and/or moderate mine burial possible but never exceeding 15 cm.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:7e2a35a8-854c-4b58-8f0e-d0f682d31a70</app:SemanticID>
            <app:DisplayName>Stable and smooth flat bottom</app:DisplayName>
            <app:PhysicalName>A</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
