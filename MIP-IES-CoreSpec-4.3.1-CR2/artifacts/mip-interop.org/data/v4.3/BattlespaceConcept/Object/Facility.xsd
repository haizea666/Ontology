            <app:SemanticID>MIM:7969f365-68c4-4343-bef9-ccdcdda274fb</app:SemanticID>
            <app:DisplayName>Towed free diver</app:DisplayName>
            <app:PhysicalName>SWTWFD</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="UnderwaterSled">
