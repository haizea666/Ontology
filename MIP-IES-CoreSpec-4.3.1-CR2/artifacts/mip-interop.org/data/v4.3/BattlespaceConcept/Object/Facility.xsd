  <xsd:element name="ApproachDirectionRunwayLightingCode" type="facility:ApproachDirectionRunwayLightingCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The type of runway lighting.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:7fd91069-ec4b-4001-920a-f2724a5e6a02</app:SemanticID>
        <app:ShortName>runwayLightingCode</app:ShortName>
        <app:MIMStereotype>code</app:MIMStereotype>
        <app:MIMStereotype>instance</app:MIMStereotype>
        <app:DisplayName>runway lighting</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="ApproachDirectionRunwayLightingCodeType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The type of runway lighting.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:efa4c943-da0c-4ae2-93d6-2b2fec3753ef</app:SemanticID>
        <app:MIMStereotype>complete</app:MIMStereotype>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="primitives:CodeValueType">
      <xsd:enumeration value="CentreLineLighting">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Lights (white and red) are located along the centre line of the runway or up to 60 cm to one side, with a spacing of 7.5 m.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:8fa887b8-988c-4098-a167-38ee03607bc7</app:SemanticID>
            <app:DisplayName>Centre line lighting</app:DisplayName>
            <app:PhysicalName>CL</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="HighIntensityRunwayLights">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Runway edge lights capable of producing a high intensity light are used to outline the edges of the runway during periods of darkness or restricted visibility conditions. The system has variable intensity controls.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:88bcd74f-e81f-47ed-a28f-5eff0db87576</app:SemanticID>
            <app:DisplayName>High intensity runway lights</app:DisplayName>
            <app:PhysicalName>HIRL</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="LowIntensityRunwayLights">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Runway edge lights capable of producing a low intensity light are used to outline the edges of runways during periods of darkness or restricted visibility conditions.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:8ae3e585-1f0b-41e4-b061-8327278a09f4</app:SemanticID>
            <app:DisplayName>Low intensity runway lights</app:DisplayName>
            <app:PhysicalName>LIRL</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="MediumIntensityRunwayLights">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Runway edge lights capable of producing a medium intensity light are used to outline the edges of runways during periods of darkness or restricted visibility conditions. The system has variable intensity controls.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:d1840b92-7bb6-44f9-9edc-4e1fa466ef6a</app:SemanticID>
            <app:DisplayName>Medium intensity runway lights</app:DisplayName>
            <app:PhysicalName>MIRL</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="RunwayEndIdentifierLights">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Runway end lights are placed on a line at right angles to the runway axis as near to the end of the runway as possible and, in any case, not more than 3 m outside the end. They are red and can only be seen in the direction of the runway.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:07a494eb-40cc-4952-8cc3-c259a293350d</app:SemanticID>
            <app:DisplayName>Runway end identifier lights</app:DisplayName>
            <app:PhysicalName>REIL</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ThresholdLighting">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Threshold lights are provided in a line along the landing threshold at the touchdown end of a runway and define the beginning of the declared Landing Distances. They are green and can only be seen from the approach.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:69bfb5b7-8934-4d8a-b54b-11555f8e5a1a</app:SemanticID>
            <app:DisplayName>Threshold lighting</app:DisplayName>
            <app:PhysicalName>TL</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="TouchdownZoneLighting">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Pairs of barrettes are symmetrically located about the runway centre line, with a spacing of 30 m or 60 m between the pairs.</xsd:documentation>
          <xsd:appinfo>
