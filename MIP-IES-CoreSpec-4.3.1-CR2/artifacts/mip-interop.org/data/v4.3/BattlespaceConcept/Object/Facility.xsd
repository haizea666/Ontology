  <xsd:simpleType name="MaritimeMinefieldSubfunctionCodeType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The intended purpose of a MaritimeMinefield.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:a1b10a46-0743-4f14-8a8c-23e576260879</app:SemanticID>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="primitives:CodeValueType">
      <xsd:enumeration value="AntiInvasion">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A maritime minefield that is primarily intended to cause damage to enemy vessels engaging in an attack on a friendly shore line.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:bc356331-ee16-4f67-b22c-729ad24699ec</app:SemanticID>
            <app:DisplayName>Anti-invasion</app:DisplayName>
            <app:PhysicalName>ANIN</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AntiPassage">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A maritime minefield that is primarily intended to cause damage to enemy vessels in a specific channel.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:1631dd24-ffa7-4c4f-bfee-6e88172dfb63</app:SemanticID>
            <app:DisplayName>Anti-passage</app:DisplayName>
            <app:PhysicalName>ANPA</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AntiShipping">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A maritime minefield that is primarily intended to cause damage to enemy ships.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:bfc81cf1-659d-4577-a34c-8d5ee1d100f7</app:SemanticID>
            <app:DisplayName>Anti-shipping</app:DisplayName>
            <app:PhysicalName>ANSH</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AntiSurfaceWarfare">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A maritime minefield that is primarily intended to cause damage to enemy surface vessels.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:cba28062-656e-4c51-91aa-ee247cc12865</app:SemanticID>
            <app:DisplayName>Anti-surface warfare</app:DisplayName>
            <app:PhysicalName>ASW</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Attrition">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A maritime minefield that is primarily intended to cause damage to enemy vessels over time.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:183dc74f-964d-4b63-b3b6-5f459b5d8bc1</app:SemanticID>
            <app:DisplayName>Attrition</app:DisplayName>
            <app:PhysicalName>ATT</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Blockade">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A maritime minefield that is primarily intended to prevent the passage of enemy vessels into or out of a specific area with respect to a fixed location on land.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:1b9523f7-6402-4a69-8765-fd201ff6069b</app:SemanticID>
            <app:DisplayName>Blockade</app:DisplayName>
            <app:PhysicalName>BLK</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Delay">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A maritime minefield that is primarily intended to impede the movement of enemy vessels.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:51f5cd07-4eb3-4fb9-90a6-6f559d14167a</app:SemanticID>
            <app:DisplayName>Delay</app:DisplayName>
            <app:PhysicalName>DEL</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Harassment">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A maritime minefield that is primarily intended to cause damage to enemy vessels over a period of time in a random manner.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:7a41ff2e-3f81-4df0-a397-4d233dc4d318</app:SemanticID>
            <app:DisplayName>Harassment</app:DisplayName>
            <app:PhysicalName>HAR</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Nuisance">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A maritime minefield that is primarily intended to cause inconvenience or annoyance to enemy vessels.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:359b76b6-2218-4a63-bb11-1fe003e04efe</app:SemanticID>
            <app:DisplayName>Nuisance</app:DisplayName>
            <app:PhysicalName>NUI</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="PortClosure">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A maritime minefield that is primarily intended to deny the usage of a harbour/port to an enemy.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:5b126507-c57c-4ad0-8566-f4ecb0a27966</app:SemanticID>
            <app:DisplayName>Port closure</app:DisplayName>
            <app:PhysicalName>PC</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="StraitClosure">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A maritime minefield that is primarily intended to deny the usage of a particular strait to enemy vessels.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:57df2c42-eaf8-4987-b050-8a0bcf9db621</app:SemanticID>
            <app:DisplayName>Strait closure</app:DisplayName>
            <app:PhysicalName>SC</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
