  </xsd:element>
  <xsd:element name="FacilityPrimaryConstructionMaterialCode" type="facility:FacilityPrimaryConstructionMaterialCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The major material used in building the Facility.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:c34f2386-9d0a-4807-98a0-8e4c2f06ac66</app:SemanticID>
        <app:ShortName>primaryConstructionMaterialCode</app:ShortName>
        <app:MIMStereotype>code</app:MIMStereotype>
        <app:MIMStereotype>instance</app:MIMStereotype>
        <app:DisplayName>primary construction material</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="FacilityPrimaryConstructionMaterialCodeType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The major material used in building a Facility.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:1d142bbd-5c4d-4724-aea6-e5b3e72ec874</app:SemanticID>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="primitives:CodeValueType">
      <xsd:enumeration value="Asphalt">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A mixture of dark bituminous pitch with sand or gravel, used for surfacing roads, flooring, roofing, etc.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:a52ba5e8-11e0-405a-94cd-ef5d7486dedd</app:SemanticID>
            <app:DisplayName>Asphalt</app:DisplayName>
            <app:PhysicalName>ASPHLT</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Bitumen">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A black viscous mixture of hydrocarbons obtained naturally or as a residue from petroleum distillation. It is used for road surfacing and roofing.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:df372fc7-63c5-4b98-a85d-fa5c9abaad05</app:SemanticID>
            <app:DisplayName>Bitumen</app:DisplayName>
            <app:PhysicalName>BIT</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BrickOrMasonry">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A small parallelepipedal block typically made of fired or sun-dried clay, used in building.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:a94bbdb4-da0e-4eee-863a-e51675dfee65</app:SemanticID>
            <app:DisplayName>Brick/Masonry</app:DisplayName>
            <app:PhysicalName>BRKMSN</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Cement">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A grey powder that is mixed with water and sand to make mortar or with water, sand, and small stones to make concrete.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:9a6f198c-704e-4543-b46e-835fa05272ae</app:SemanticID>
            <app:DisplayName>Cement</app:DisplayName>
            <app:PhysicalName>CEM</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Cinder">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The residue of a combustible substance, esp. coal, after it has ceased to flame or burn.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:f8f66dc9-96f7-40b3-a71e-d6a4a4fc2747</app:SemanticID>
            <app:DisplayName>Cinder</app:DisplayName>
            <app:PhysicalName>CND</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Clay">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A stiff, sticky fine-grained earth that can be moulded when wet, and is dried and baked to make bricks, pottery, and ceramics.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:bd282b71-5ca7-4ed1-a90c-15e1c1395b90</app:SemanticID>
            <app:DisplayName>Clay</app:DisplayName>
            <app:PhysicalName>CLA</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Cobblestone">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Small round stones used to cover road surfaces.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:b26fb937-1b5d-4632-88f8-5d4fb988b5e5</app:SemanticID>
            <app:DisplayName>Cobblestone</app:DisplayName>
            <app:PhysicalName>CBLSTN</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Concrete">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A building material made from a mixture of broken stone or gravel, sand, cement, and water, which can be spread or poured into moulds and forms a mass resembling stone on hardening.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:ddd8797f-6e33-4c2f-b269-42f4e2669480</app:SemanticID>
            <app:DisplayName>Concrete</app:DisplayName>
            <app:PhysicalName>CNCRTE</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ConcreteBlocks">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Pre-constructed pieces from a composition of gravel, sand, cement, and water, used for building.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:94a0f74a-835c-485f-8860-45f3347eb342</app:SemanticID>
            <app:DisplayName>Concrete blocks</app:DisplayName>
            <app:PhysicalName>CNCRBL</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Coral">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A hard stony substance secreted by certain marine coelenterates as an external skeleton, typically forming large reefs in warm seas.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:60edfaed-5ca7-4d7b-a0d1-2819687fa5e9</app:SemanticID>
            <app:DisplayName>Coral</app:DisplayName>
            <app:PhysicalName>COR</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Earth">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A black or dark brown material typically consisting of a mixture of organic remains, clay, and rock particles that constitutes the upper layer of earth in which plants grow.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:18e0df4a-ed07-4585-884c-9045c06cc03a</app:SemanticID>
            <app:DisplayName>Earth</app:DisplayName>
            <app:PhysicalName>EARTH</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Grass">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Vegetation consisting of typically short plants with long, narrow leaves, growing wild or cultivated on lawns and pasture, and as a fodder crop.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:b8e26571-cfe6-4c99-95db-81e7e6ab4003</app:SemanticID>
            <app:DisplayName>Grass</app:DisplayName>
            <app:PhysicalName>GRS</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Gravel">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A loose aggregation of small water-worn or pounded stones.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:65e5d4f5-8ac8-4a97-8880-0a7000b405e7</app:SemanticID>
            <app:DisplayName>Gravel</app:DisplayName>
            <app:PhysicalName>GRAVEL</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Gypsum">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Hydrous calcium sulphate, the mineral from which plaster of Paris is made.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:a8b466c5-d1a5-49eb-a3b2-72c95b2cf262</app:SemanticID>
            <app:DisplayName>Gypsum</app:DisplayName>
            <app:PhysicalName>GYP</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Ice">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Frozen water, a brittle transparent crystalline solid.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:2c2c5b98-fdf3-418e-8936-c859b6b9b682</app:SemanticID>
            <app:DisplayName>Ice</app:DisplayName>
            <app:PhysicalName>ICE</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Laterite">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A reddish clayey material, hard when dry, forming a topsoil in some tropical or subtropical regions and sometimes used for building.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:e8735243-447c-4fe6-940a-f59224c8207a</app:SemanticID>
            <app:DisplayName>Laterite</app:DisplayName>
            <app:PhysicalName>LAT</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Limestone">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A rock which consists chiefly of carbonate of lime, and yields lime when burnt.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:491fb9b9-fd23-44d6-bdb7-d57d2ba715aa</app:SemanticID>
            <app:DisplayName>Limestone</app:DisplayName>
            <app:PhysicalName>LST</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Macadam">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Broken stone of even size, bound with tar or bitumen and used in successively compacted layers for surfacing roads and paths.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:85443094-d851-4408-a753-4ca1c7b8478c</app:SemanticID>
            <app:DisplayName>Macadam</app:DisplayName>
            <app:PhysicalName>MACDAM</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Membrane">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A thin sheet of tissue (plastic or other coated fibre material) acting as a boundary or lining in a construction.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:664cb12c-6c5a-464f-9143-762a9f79fe67</app:SemanticID>
            <app:DisplayName>Membrane</app:DisplayName>
            <app:PhysicalName>MEM</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Metal">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A solid material which is typically hard, shiny, malleable, fusible, and ductile, with good electrical and thermal conductivity (e.g. iron, gold, silver, and aluminium, and alloys such as steel).</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:5a2d3ed4-5952-4fd2-961f-d18d3acd65df</app:SemanticID>
            <app:DisplayName>Metal</app:DisplayName>
            <app:PhysicalName>METAL</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Mix">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A variety of materials mixed in place using non-bituminous binders such as portland cement.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:2625ad94-7f20-4933-9199-da9e1470875b</app:SemanticID>
            <app:DisplayName>Mix</app:DisplayName>
            <app:PhysicalName>MIX</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Pebble">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Small stones made smooth and round by the action of water or sand.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:849553ef-2272-4db2-ba67-15e3814aec94</app:SemanticID>
            <app:DisplayName>Pebble</app:DisplayName>
            <app:PhysicalName>PEBBLE</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="PermanentComposite">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">An assembly of permanent (hard) surface material and other materials covering the Facility surface, where the hard material constitutes 50 percent or more of the surface length of the Facility.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:e26dd894-e629-41d8-a934-6e75350e7d86</app:SemanticID>
            <app:DisplayName>Composite, permanent</app:DisplayName>
            <app:PhysicalName>COP</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="PermanentSurface">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A permanent (hard) surface material of unknown type, covering the Facility surface.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:3d74f118-7318-4312-808f-7c35d5326570</app:SemanticID>
            <app:DisplayName>Permanent surface</app:DisplayName>
            <app:PhysicalName>PER</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="PermanentSurfaceMix">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A permanent (hard) surface material, made up of part concrete, part asphalt, or part bitumen-bound macadam, covering the Facility surface.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:fffa3269-281f-4354-bf1e-49ff748a653d</app:SemanticID>
            <app:DisplayName>Permanent surface mix</app:DisplayName>
            <app:PhysicalName>PEM</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="PiercedAluminiumPlank">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A strip of aluminium perforated with regularly spaced lightening holes.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:faca2e7c-09c3-40b4-89f4-f6e139517669</app:SemanticID>
            <app:DisplayName>Pierced aluminium plank</app:DisplayName>
            <app:PhysicalName>PAP</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="PiercedSteelPlank">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A strip of steel perforated with regularly spaced lightening holes.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:1729b0de-1bb6-4dc2-a13b-ed56faef7fc9</app:SemanticID>
            <app:DisplayName>Pierced steel plank</app:DisplayName>
            <app:PhysicalName>PSP</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="PreStressedConcrete">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Pre-constructed pieces from a composition of gravel, sand, cement, and water, strengthened by the application of stress during manufacture, especially by means of rods or wires inserted under tension before the material is set.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:471b223c-91ee-4f6d-af33-85d226729c1f</app:SemanticID>
            <app:DisplayName>Pre-stressed concrete</app:DisplayName>
            <app:PhysicalName>PRSTCN</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ReinforcedConcrete">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A composition of gravel, sand, cement, and water, strengthened with additional material.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:67616d67-d8ec-4bb7-9999-76d28e2ca7c0</app:SemanticID>
            <app:DisplayName>Reinforced concrete</app:DisplayName>
            <app:PhysicalName>REINCN</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Rock">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A solid mineral material forming part of the surface of the earth and other similar planets, exposed on the surface or underlying the soil.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:07d437e0-bce1-4eb1-9a5a-6ef782d2b199</app:SemanticID>
            <app:DisplayName>Rock</app:DisplayName>
            <app:PhysicalName>ROCK</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="RolledEarth">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A black or dark brown material typically consisting of a mixture of organic remains, clay, and rock particles, compressed and flattened under a heavy cylinder.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:fe6a9ddb-7fd1-4e7b-815b-cfc5c3ff34ba</app:SemanticID>
            <app:DisplayName>Rolled earth</app:DisplayName>
            <app:PhysicalName>RLDERT</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Sand">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A loose granular substance, typically pale yellowish brown, resulting from the erosion of siliceous and other rocks and forming a major constituent of beaches, river beds, the seabed, and deserts.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:31b81e16-f723-4f81-8b28-f68996860c14</app:SemanticID>
            <app:DisplayName>Sand</app:DisplayName>
            <app:PhysicalName>SAND</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Silt">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Fine sand, clay, or other material carried by running water and deposited as a sediment, especially in a channel or harbour.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:95934dd5-ae86-4ed9-9f45-b8cc6e036965</app:SemanticID>
            <app:DisplayName>Silt</app:DisplayName>
            <app:PhysicalName>SILT</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Snow">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Atmospheric water vapour frozen into ice crystals, lying on the ground as a white layer.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:8f5e433b-8c75-4c9c-be66-422363f536e8</app:SemanticID>
            <app:DisplayName>Snow</app:DisplayName>
            <app:PhysicalName>SNOW</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Sod">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A piece or slice of earth held together by the roots of the grass growing on it, usually square or oblong in shape and of moderate thickness, cut out or pared off from the surface of grass land.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:1c28b774-fd0a-42b0-8301-1fc65c3f41d3</app:SemanticID>
            <app:DisplayName>Sod</app:DisplayName>
            <app:PhysicalName>SOD</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="SoftComposite">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">An assembly of permanent (hard) surface material and other materials covering the Facility surface, where the hard material constitutes less than 50 percent of the surface length of the Facility.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:cff9e9b2-3014-407e-8029-c9195324277b</app:SemanticID>
            <app:DisplayName>Composite, soft</app:DisplayName>
            <app:PhysicalName>COM</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="SteelMat">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A sheet of steel covering the surface of the Facility.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:792b8e40-250a-495a-b857-d7e8aeeb8fa6</app:SemanticID>
            <app:DisplayName>Steel mat</app:DisplayName>
            <app:PhysicalName>STELMT</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Turf">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A slab pared from the surface of the soil with the grass and herbage growing on it.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:afec081a-d965-4982-b736-41096f3fa122</app:SemanticID>
            <app:DisplayName>Turf</app:DisplayName>
            <app:PhysicalName>TRF</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="WoodOrTimber">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The hard fibrous material that forms the main substance of the trunk or branches of a tree or shrub, used for building or carpentry.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:ebdea434-aee4-442b-adba-0a25adfb762b</app:SemanticID>
            <app:DisplayName>Wood/timber</app:DisplayName>
            <app:PhysicalName>WOOD</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:element name="FacilityPrimaryFacility" type="facility:FacilityType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The subject (secondary) Facility is secondary to the object (primary) Facility, i.e. it is expected to be used only when the primary facility is not fully used or if the latter is no longer able to operate.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:d116b477-e0ee-4041-9621-eae2c0c0621f-2I</app:SemanticID>
        <app:ShortName>primaryFacility</app:ShortName>
        <app:MIMStereotype>instance</app:MIMStereotype>
        <app:MIMStereotype>structure</app:MIMStereotype>
        <app:DisplayName>is secondary to</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="FacilityPrimaryFacilityRef" type="base:IdentifiableReferenceType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The subject (secondary) Facility is secondary to the object (primary) Facility, i.e. it is expected to be used only when the primary facility is not fully used or if the latter is no longer able to operate.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:d116b477-e0ee-4041-9621-eae2c0c0621f-2R</app:SemanticID>
        <app:ShortName>primaryFacilityRef</app:ShortName>
        <app:MIMStereotype>instance</app:MIMStereotype>
        <app:MIMStereotype>structure</app:MIMStereotype>
        <app:ReferenceType>MIM:3b27d26e-8432-43e1-bd8f-582c0165be5a</app:ReferenceType>
        <app:DisplayName>is secondary to</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="FacilityRepairedMateriel" type="materiel:MaterielType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The Facility restores the Materiel to sound condition after damage or failure.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:665697d0-2fc1-4b42-b767-68cf0005e806-2I</app:SemanticID>
        <app:ShortName>repairedMateriel</app:ShortName>
        <app:MIMStereotype>instance</app:MIMStereotype>
        <app:MIMStereotype>structure</app:MIMStereotype>
        <app:DisplayName>repairs</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
