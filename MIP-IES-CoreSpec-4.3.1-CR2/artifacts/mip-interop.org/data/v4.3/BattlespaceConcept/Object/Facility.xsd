      <xsd:documentation xml:lang="en">The class of approach lighting system.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:55cea902-a1c1-42d1-823e-040b0bec4946</app:SemanticID>
        <app:MIMStereotype>complete</app:MIMStereotype>
        <app:MIMStereotype>ordered</app:MIMStereotype>
        <app:OrderRelation>allows less precise approach procedures than</app:OrderRelation>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="primitives:CodeValueType">
      <xsd:enumeration value="SimpleApproachLightingSystem">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">An approach lighting system consisting of a row of lights on the extended centreline of the runway over a distance of no less than 420 m, with another set of lights forming a crossbar 18 m or 30 m in length at a distance of 300 m from the runway's threshold.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:24dd1ec9-4712-4e64-be06-5df9afb7e7eb</app:SemanticID>
            <app:DisplayName>Simple approach lighting system</app:DisplayName>
            <app:PhysicalName>SimpleALS</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="PrecisionApproachCategoryILightingSystem">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">An approach lighting system consisting of a row of lights on the extended centreline of the runway over a distance of no less than 900 m, with another set of lights forming a crossbar 30 m in length at a distance of 300 m from the runway's threshold.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:5a78bfb9-41f2-4895-b582-e668f947bf55</app:SemanticID>
            <app:DisplayName>Precision approach category I lighting system</app:DisplayName>
            <app:PhysicalName>CAT1ALS</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="PrecisionApproachCategoryIIAndIIILightingSystem">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">An approach lighting system consisting of a row of lights on the extended centreline of the runway over a distance of no less than 900 m. In addition, the system has two side rows of lights extending 270 m from the threshold, and two crossbars, one at 150 m and one at 300 m from the threshold.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:57cd87de-bd42-4d29-80ad-f6a0f4f1aeb0</app:SemanticID>
            <app:DisplayName>Precision approach category II and III lighting system</app:DisplayName>
            <app:PhysicalName>CAT2-3ALS</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
