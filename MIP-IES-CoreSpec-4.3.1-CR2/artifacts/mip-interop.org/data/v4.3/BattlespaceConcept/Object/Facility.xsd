            <app:PhysicalName>SWMEOR</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="SweptMechanicalSnagline">
        <xsd:annotation>
