            <app:DisplayName>Swept mechanical team</app:DisplayName>
            <app:PhysicalName>SWMETE</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="TowedFreeDiver">
