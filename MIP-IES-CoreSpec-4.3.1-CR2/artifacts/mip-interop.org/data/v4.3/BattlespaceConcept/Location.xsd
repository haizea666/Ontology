        <app:MIMStereotype>structure</app:MIMStereotype>
        <app:DisplayName>longitude coordinate</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="AbsolutePointType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">A Point that has its position specified with respect to the surface of the World Geodetic System 1984 (WGS 84) ellipsoid.</xsd:documentation>
