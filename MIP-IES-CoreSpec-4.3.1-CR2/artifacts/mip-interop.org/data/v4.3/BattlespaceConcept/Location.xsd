      <xsd:documentation xml:lang="en">The speed of an object relative to its normal speed.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:bdfd8731-836a-4426-9972-9745904ffcbf</app:SemanticID>
        <app:MIMStereotype>complete</app:MIMStereotype>
        <app:MIMStereotype>ordered</app:MIMStereotype>
        <app:OrderRelation>is lower relative speed than</app:OrderRelation>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="primitives:CodeValueType">
      <xsd:enumeration value="NoSpeed">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Self explanatory.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:a3285eeb-8200-40dd-9824-a7de2d741e66</app:SemanticID>
