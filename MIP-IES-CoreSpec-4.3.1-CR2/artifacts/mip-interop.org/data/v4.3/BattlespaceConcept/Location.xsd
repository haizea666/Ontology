        <app:DisplayName>speed</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="VelocityType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The movement of an Object in terms of speed and direction.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:fdb2ce6d-40a6-4df0-b397-41befd10faf6</app:SemanticID>
        <app:DisplayName>Velocity</app:DisplayName>
