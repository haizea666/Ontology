        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Slow">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Slower than the normal speed of the object.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:962b0c6d-4f99-4db8-b910-3d9a85f5e2e4</app:SemanticID>
            <app:DisplayName>Slow</app:DisplayName>
            <app:PhysicalName>SLOW</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
