      <xsd:extension base="location:PointType">
        <xsd:sequence>
          <xsd:element ref="location:AbsolutePointLatitudeCoordinate" />
          <xsd:element ref="location:AbsolutePointLongitudeCoordinate" />
          <xsd:element ref="location:AbsolutePointVerticalDistance" minOccurs="0" />
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
