        <xsd:sequence>
          <xsd:element ref="location:VelocityAbsoluteBearingAngle" minOccurs="0" />
          <xsd:element ref="location:VelocityApproximateSpeedCode" minOccurs="0" />
          <xsd:element ref="location:VelocityInclinationAngle" minOccurs="0" />
          <xsd:element ref="location:VelocitySpeed" minOccurs="0" />
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
