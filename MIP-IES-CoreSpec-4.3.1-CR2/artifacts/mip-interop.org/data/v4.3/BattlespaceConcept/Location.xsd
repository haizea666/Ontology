  <xsd:complexType name="ArrowType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">A graphical representation of a BattlespaceConcept that is constituted of at least 2 ordered points and an optional width.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:8f6c6cdc-d604-4bcc-b4c8-0d627745cacc</app:SemanticID>
        <app:Remarks>The graphic is decorated around a (possibly multi-segment) arrow with an optional width.

	* Point 1 defines the head of the arrow.
	* Points 2..n define the path of the arrow.</app:Remarks>
        <app:DisplayName>Arrow</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="location:GraphicType">
        <xsd:sequence>
          <xsd:element ref="location:ArrowPoint" minOccurs="2" maxOccurs="unbounded" />
          <xsd:element ref="location:ArrowWidthDimension" minOccurs="0" />
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
