      <xsd:enumeration value="Medium">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The normal speed of the object.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:553be866-6d50-4fb0-9ee0-ab230d3df9e3</app:SemanticID>
            <app:DisplayName>Medium</app:DisplayName>
            <app:PhysicalName>MEDIUM</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Fast">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Faster than the normal speed of the object.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:fd110ca4-403f-473b-98c2-040a04d60e80</app:SemanticID>
            <app:DisplayName>Fast</app:DisplayName>
            <app:PhysicalName>FAST</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:element name="VelocityInclinationAngle" type="location:AngleType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The rotational measurement from the horizontal plane to the direction of motion of the Object, where the positive angle is vertically upward.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:44d91000-3554-4434-97c1-ebe9df4c0657</app:SemanticID>
        <app:ShortName>inclinationAngle</app:ShortName>
