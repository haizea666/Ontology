        <app:SemanticID>MIM:8e492899-4717-409d-bfd1-22f06c11fa08</app:SemanticID>
        <app:ShortName>widthDimension</app:ShortName>
        <app:Remarks>If the width is unspecified, the Arrow is drawn by a single line.</app:Remarks>
        <app:MIMStereotype>dimension</app:MIMStereotype>
        <app:MIMStereotype>instance</app:MIMStereotype>
        <app:DisplayName>width</app:DisplayName>
        <app:UnitOfMeasure>metre (m)</app:UnitOfMeasure>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
