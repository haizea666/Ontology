      <xsd:documentation xml:lang="en">The motion of the Object in terms of horizontal distance per unit of time.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:e41f60cd-31ac-4f7d-a159-2b6e294acb89</app:SemanticID>
        <app:ShortName>speed</app:ShortName>
        <app:MIMStereotype>instance</app:MIMStereotype>
