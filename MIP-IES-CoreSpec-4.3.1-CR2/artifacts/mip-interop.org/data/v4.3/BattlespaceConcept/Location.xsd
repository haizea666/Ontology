      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="AnglePrecisionCodeType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The maximum resolution used for the expression of the value of an angle.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:38b1ed26-04de-4e76-a235-137ce1f0447d</app:SemanticID>
        <app:MIMStereotype>complete</app:MIMStereotype>
        <app:MIMStereotype>ordered</app:MIMStereotype>
        <app:OrderRelation>is lower precision than</app:OrderRelation>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="primitives:CodeValueType">
      <xsd:enumeration value="Degree">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Angular precision is expressed to the precision of a degree (60 minutes).</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:24e0fbce-7b2e-44e7-bd30-0d4c12071cd9</app:SemanticID>
            <app:DisplayName>Degree</app:DisplayName>
            <app:PhysicalName>DEGREE</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="_1_10thOfADegree">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Angular precision is expressed to the precision of 1/10 of a degree.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:63fcec21-e028-4a88-848a-3ba103d6237d</app:SemanticID>
            <app:DisplayName>1/10 degree</app:DisplayName>
            <app:PhysicalName>DEG10</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Mil">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Angular precision is expressed to the precision of 1 mil (1/6400 of a full circle).</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:6eb637e7-83ed-45a5-832c-08c25d066ab7</app:SemanticID>
            <app:DisplayName>Mil</app:DisplayName>
            <app:PhysicalName>MIL</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="AngleType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The rotational measurement between two lines and/or planes diverging from a common point and/or line, measured with a range of uncertainty and a maximum resolution.</xsd:documentation>
