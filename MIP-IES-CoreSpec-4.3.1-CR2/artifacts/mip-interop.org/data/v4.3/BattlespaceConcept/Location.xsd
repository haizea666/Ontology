      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="VerticalDistanceDimensionType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The distance with respect to the specified vertical datum.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:96168a4b-b5be-40d9-862a-32a31af89b69</app:SemanticID>
        <app:ShortName>dimension</app:ShortName>
        <app:ValueOrAmplification>1</app:ValueOrAmplification>
