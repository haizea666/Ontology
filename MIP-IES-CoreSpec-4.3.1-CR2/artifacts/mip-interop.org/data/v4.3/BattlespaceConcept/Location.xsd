  <xsd:element name="VerticalDistancePrecisionCode" type="location:DistancePrecisionCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The precision of the VerticalDistance.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:997222fd-2e61-4c90-ab0e-30240b20ab67</app:SemanticID>
