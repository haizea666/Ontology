        <app:MIMStereotype>structure</app:MIMStereotype>
        <app:DisplayName>latitude coordinate</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AbsolutePointLongitudeCoordinate" type="location:LongitudeCoordinateType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The angle, defined with a specific resolution, between the initial (zero or Greenwich) meridian and the meridian of the point measured in the plane of the Equator.</xsd:documentation>
