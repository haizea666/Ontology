            <app:PhysicalName>AIRDEF</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AntiArmour">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The intended result is to damage or destroy armoured vehicles and/or to kill or wound individuals inside armoured vehicles.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:9f9d5eb1-cc3a-4611-abfd-c3dab4927e14</app:SemanticID>
            <app:DisplayName>Anti-armour</app:DisplayName>
            <app:PhysicalName>ARMDEF</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AntiInfrastructure">
