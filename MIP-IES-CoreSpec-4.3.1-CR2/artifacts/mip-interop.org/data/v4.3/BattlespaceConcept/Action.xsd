        <app:SemanticID>MIM:fe7e475f-3591-4746-bb3f-dad10f73a80f</app:SemanticID>
        <app:ShortName>useSequenceCode</app:ShortName>
        <app:MIMStereotype>code</app:MIMStereotype>
        <app:MIMStereotype>instance</app:MIMStereotype>
        <app:DisplayName>use sequence</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="IEDEventUseSequenceCodeType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The enemy use of an IED as a primary, secondary, or tertiary form of attack, independent of the intended outcome.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:e3374441-5727-4082-b419-b99e34466ee8</app:SemanticID>
        <app:MIMStereotype>complete</app:MIMStereotype>
        <app:MIMStereotype>ordered</app:MIMStereotype>
