          <xsd:documentation xml:lang="en">The intended result is to damage or destroy physical infrastructure.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:4cdc8601-f429-4451-b6a3-54ae8ab43a0f</app:SemanticID>
            <app:DisplayName>Anti-infrastructure</app:DisplayName>
            <app:PhysicalName>INFDEF</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AntiPersonnel">
