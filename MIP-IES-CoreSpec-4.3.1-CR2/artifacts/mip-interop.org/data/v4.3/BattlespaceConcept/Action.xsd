            <app:DisplayName>Vehicle-borne</app:DisplayName>
            <app:PhysicalName>VBIED</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="VesselBorne">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The IED is delivered by any vessel that serves as the concealment means for explosives with an Initiating device.</xsd:documentation>
