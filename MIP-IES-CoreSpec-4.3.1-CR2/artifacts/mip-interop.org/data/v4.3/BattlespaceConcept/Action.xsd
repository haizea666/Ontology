            <app:PhysicalName>PRI</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Secondary">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">This is an additional device emplaced in the target area to attack individuals or vehicles after the initial event.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:d00d9ede-ec5e-4782-b3c5-93c4c49adf81</app:SemanticID>
            <app:DisplayName>Secondary</app:DisplayName>
            <app:PhysicalName>SEC</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Tertiary">
