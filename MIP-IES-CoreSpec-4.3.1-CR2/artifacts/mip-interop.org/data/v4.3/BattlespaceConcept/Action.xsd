  <xsd:element name="IEDEventUseSequenceCode" type="action:IEDEventUseSequenceCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The enemy use of an IED as a primary, secondary, or tertiary form of attack, independent of the intended outcome.</xsd:documentation>
