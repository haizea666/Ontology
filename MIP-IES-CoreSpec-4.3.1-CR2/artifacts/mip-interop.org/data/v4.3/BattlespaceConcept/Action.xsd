      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="primitives:CodeValueType">
      <xsd:enumeration value="Abseil">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">To deplane using rappelling equipment while the aircraft is airborne.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:bb0bc946-2a7d-426c-b0ed-570c737678fd</app:SemanticID>
            <app:DisplayName>Abseil</app:DisplayName>
            <app:PhysicalName>ABSEIL</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AutomaticParachute">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">To deplane from the aircraft while it is airborne with a parachute that will unfold itself automatically after leaving the aircraft.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:8882fbef-2a1b-47e0-9656-72975dd755ec</app:SemanticID>
            <app:DisplayName>Parachute, automatic</app:DisplayName>
            <app:PhysicalName>PARAUT</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ExtractionParachute">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">To deplane from the aircraft while it is airborne with a parachute that will pull the cargo out of the aircraft, and then unfold the main parachute.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:5760f4bb-ad3e-441f-a079-85643024663f</app:SemanticID>
            <app:DisplayName>Parachute, extraction</app:DisplayName>
            <app:PhysicalName>PAREXT</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Gravity">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">To deplane cargo without a parachute from the aircraft while it is airborne by gravitational force.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:54d4b31f-5e0d-46c8-a689-83bf5f46e8b7</app:SemanticID>
