  <xsd:element name="ActionElectronicWarfareEmploymentCategoryCode" type="action:ActionElectronicWarfareEmploymentCategoryCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The electronic or mechanical technique.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:aacc12ef-79cd-40b4-a931-f99515a74360</app:SemanticID>
        <app:ShortName>categoryCode</app:ShortName>
        <app:ValueOrAmplification>1</app:ValueOrAmplification>
        <app:MIMStereotype>code</app:MIMStereotype>
        <app:MIMStereotype>discriminator</app:MIMStereotype>
        <app:MIMStereotype>instance</app:MIMStereotype>
        <app:DisplayName>category</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="ActionElectronicWarfareEmploymentCategoryCodeType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">An electronic or mechanical technique.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:e3e92046-e5b3-474b-be13-e600bb71e29b</app:SemanticID>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="primitives:CodeValueType">
      <xsd:enumeration value="AcousticSimulationOfSurfaceAndSubsurfaceForces">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Imitating or controlling the signature of a ship to mislead or deceive the enemy.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:fae3234c-793f-4af2-a93a-8a3d74e6ec0f</app:SemanticID>
            <app:DisplayName>Acoustic simulation of surface and subsurface forces</app:DisplayName>
            <app:PhysicalName>ACOUST</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AmphibiousWarfareSimulation">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Simulate sounds or noises that resemble an amphibious operation such as dropping anchors, starting boat engines, etc.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:d25187d3-f25e-47be-b6f4-67871a454f3b</app:SemanticID>
            <app:DisplayName>Simulation, amphibious warfare</app:DisplayName>
            <app:PhysicalName>SIMAMP</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BarrageJam">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The simultaneous jamming of a number of adjacent channels or frequencies.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:97264af7-a5c3-4bcc-b63a-987ff67cbf58</app:SemanticID>
            <app:DisplayName>Jam, barrage</app:DisplayName>
            <app:PhysicalName>JAMBAR</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BlanketChaff">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Employed to offset unfriendly radar detection capability by dispensing a blanket of chaff over a designated area.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:8fe8d66b-8b79-49e5-8270-bd35eb4b1ef3</app:SemanticID>
            <app:DisplayName>Chaff, blanket</app:DisplayName>
            <app:PhysicalName>CHAFBL</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BurstChaff">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Chaff drops at intervals short enough to appear on radar displays as an individual target.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:a4f5cbaa-9696-4296-b6b1-8f4278495ea5</app:SemanticID>
            <app:DisplayName>Chaff, burst</app:DisplayName>
            <app:PhysicalName>CHAFBU</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="CommunicationsNoiseJam">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The broadcast or rebroadcast of any type of noise or other transmission intended to render the victim communications frequency unusable.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:012c85dc-ae5a-4998-b333-25effe774dfb</app:SemanticID>
            <app:DisplayName>Jam, communications noise</app:DisplayName>
            <app:PhysicalName>JAMCOM</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ConcealmentOfForces">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Can be accomplished by using sounds to mask unavoidable operational noises.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:554280ef-99f9-466a-8bc2-0544280a6739</app:SemanticID>
            <app:DisplayName>Concealment of forces</app:DisplayName>
            <app:PhysicalName>CONCEL</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ControlledBreachesOfCommunicationSecurity">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The deliberate act of conveying through friendly communications false or misleading information to support an overall cover and deception plan.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:c8c117d0-a141-43e0-b8f0-5697afc41b0d</app:SemanticID>
            <app:DisplayName>Controlled breaches of communication security</app:DisplayName>
            <app:PhysicalName>CONTRL</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="CornerReflector">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Consists of flat reflecting surfaces, of various shapes and sizes, connected to form a three-dimensional reflector.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:710a78bf-fb76-4e53-8301-da41d74de339</app:SemanticID>
            <app:DisplayName>Reflector, corner</app:DisplayName>
            <app:PhysicalName>RFLCCR</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="CorridorChaff">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">To offset unfriendly radar detection capability by dispensing a heavy continuous trail of chaff from an aircraft in a corridor fashion as it approaches the radar.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:a535a471-7384-4634-9c80-4ef2a531dea4</app:SemanticID>
            <app:DisplayName>Chaff, corridor</app:DisplayName>
            <app:PhysicalName>CHAFCO</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="CryptographicIntrusion">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A sophisticated imitative communications deception (ICD) technique whereby SIGINT personnel gain access to the enemy’s communication system and introduce bogus messages enciphered in the enemy’s own crypto system.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:5d0fd3ac-61e0-4a9d-bb09-8703e97a07fb</app:SemanticID>
            <app:DisplayName>Cryptographic intrusion</app:DisplayName>
            <app:PhysicalName>CRYPTO</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DeceptiveJam">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A method of transmitting signals which will prevent or reduce the enemy’s use of the communication portion of the electromagnetic spectrum, without his being aware of the source of emissions.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:4d63ae38-0f01-4e1a-9b59-abee567c51ae</app:SemanticID>
            <app:DisplayName>Jam, deceptive</app:DisplayName>
            <app:PhysicalName>JAMDEC</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ElectronicCover">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The variance of location and usage of emitters in tactical units to alter the stereotype patterns associated with those electromagnetic signatures.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:6381a52b-8a59-4971-bb2c-ba6ae97781c9</app:SemanticID>
            <app:DisplayName>Electronic cover</app:DisplayName>
            <app:PhysicalName>ELECCO</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="FalseActivityDeceive">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The increase or decrease of friendly activity.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:1dbac469-f9f0-4498-b1f1-4318c935d2ce</app:SemanticID>
            <app:DisplayName>Deceive, false activity</app:DisplayName>
            <app:PhysicalName>DCFLAC</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="FalsePeaksDeceive">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">This technique is the reverse of false traffic levels and serves the purpose of simulating unusual activity on a communication net.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:e5a960ac-b8f4-445d-8be5-03fb74522c14</app:SemanticID>
            <app:DisplayName>Deceive, false peaks</app:DisplayName>
            <app:PhysicalName>DCFLPK</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="FalseTargetGenerationOrSpoofingDeceive">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A method of deceiving the enemy by creating false targets.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:e0fcfb53-9680-4b2f-b8ac-be62b64b3da3</app:SemanticID>
            <app:DisplayName>Deceive, false target generation or spoofing</app:DisplayName>
            <app:PhysicalName>DCFLTA</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="FalseTrafficLevelsDeceive">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">This technique is applied on a communications net over a period of days to create the impression of normal operations.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:04883eb6-35dc-4cff-aabe-69edcdd365a9</app:SemanticID>
            <app:DisplayName>Deceive, false traffic levels</app:DisplayName>
            <app:PhysicalName>DCFLTR</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ForceSimulation">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">This technique can be achieved by projecting selected audio signals or by emitting sounds from specific areas.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:18a19997-205b-4fc2-bcad-e20f136bd06a</app:SemanticID>
            <app:DisplayName>Simulation, force</app:DisplayName>
            <app:PhysicalName>SIMFRC</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="GullReflector">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A floating radar reflector used to simulate surface targets.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:14f3a675-a3f4-4b2a-b4c9-de0ddd9e8e5c</app:SemanticID>
            <app:DisplayName>Reflector, gull</app:DisplayName>
            <app:PhysicalName>RLLCGL</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="InverseGainModulation">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The method of breaking a lock on a conical scan tracking radar by providing false azimuth information.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:a4b20226-5f1e-4bd9-a147-e15e0d3c8807</app:SemanticID>
            <app:DisplayName>Inverse gain modulation</app:DisplayName>
            <app:PhysicalName>INVGNM</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="MultipleFalseTargetsDeceive">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Used to generate multiple false targets.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:a55f4a30-8a8c-468f-874c-5f2dd9ffd138</app:SemanticID>
            <app:DisplayName>Deceive, multiple false targets</app:DisplayName>
            <app:PhysicalName>DCMLFL</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="NuisanceIntrusion">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">An ICD technique whereby SIGINT/EW personnel gain entry into the enemy’s communications net as a bonafide subscriber, to harass and distract the enemy.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:0493bd23-ac47-41bf-8fbc-47492774b267</app:SemanticID>
            <app:DisplayName>Nuisance intrusion</app:DisplayName>
            <app:PhysicalName>NUISNC</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Padding">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">This method involves the insertion of dummy code or cipher groups into individual valid encrypted messages.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:f9542bad-6cf0-4461-9881-96a6a5dc34b3</app:SemanticID>
            <app:DisplayName>Padding</app:DisplayName>
            <app:PhysicalName>PADDNG</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Pill">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A device released by a submarine to create false sonar targets by generating clouds of air or gas bubbles.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:3ca39a5a-f5f6-4f66-9309-64a3487b3c54</app:SemanticID>
            <app:DisplayName>Pill</app:DisplayName>
            <app:PhysicalName>PILL</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="PlannedMessageIntrusion">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A technique similar to nuisance intrusion with the intent to introduce valid enemy traffic into the enemy’s communication network.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:647bf54f-c94c-4364-827b-199cdf6edda1</app:SemanticID>
            <app:DisplayName>Planned message intrusion</app:DisplayName>
            <app:PhysicalName>PLNMSG</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="RangeGatePullOff">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">This technique involves stealing the tracking radars timing gate to disrupt the tracking function.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:ef81337e-c3fb-4e64-b6f3-24f1691fc734</app:SemanticID>
            <app:DisplayName>Range gate pull-off</app:DisplayName>
            <app:PhysicalName>RANGGT</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="RepeaterJam">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The intercept of the intended victim signal, which is then altered and retransmitted for the purpose of falsifying or disrupting information flow.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:42184129-e811-4a6d-8fe8-37a871708328</app:SemanticID>
            <app:DisplayName>Jam, repeater</app:DisplayName>
            <app:PhysicalName>JAMREP</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="RiverineWarfareSimulation">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Simulate river patrol operations.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:dc99d9d5-dfaa-498a-9223-2f7b94a045ae</app:SemanticID>
            <app:DisplayName>Simulation, riverine warfare</app:DisplayName>
            <app:PhysicalName>SIMRIV</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Routing">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Deceptive method of concealing the routing of traffic by sending to stations other than the intended recipients.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:a687b77a-9128-49a2-919c-a21001e65487</app:SemanticID>
            <app:DisplayName>Routing</app:DisplayName>
            <app:PhysicalName>ROUTNG</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ScanRateModulation">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">This technique causes the enemy radar antenna to loose lock on target.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:7dc31840-5333-4040-9f83-90464e8e2d3b</app:SemanticID>
            <app:DisplayName>Scan rate modulation</app:DisplayName>
            <app:PhysicalName>SCANRT</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="SelfProtectionChaff">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Employed to offset unfriendly radar detection and associated weapons employment by dispensing singular or limited bursts of chaff.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:f286f50d-b42e-4c4c-bcab-70e0701755df</app:SemanticID>
            <app:DisplayName>Chaff, self-protection</app:DisplayName>
            <app:PhysicalName>CHAFSE</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="SelfProtectionElectronicWarfare">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A method of transmitting radiation to prevent enemy detection.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:e9497616-b420-4572-acff-bfc14e93d1c0</app:SemanticID>
            <app:DisplayName>Electronic warfare, self-protection</app:DisplayName>
            <app:PhysicalName>EWSELF</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="SpotJam">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The jamming of a specific channel or frequency.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:f2973431-517b-4c87-a4aa-d080051ee0e2</app:SemanticID>
            <app:DisplayName>Jam, spot</app:DisplayName>
            <app:PhysicalName>JAMSPT</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="SweepJam">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Used for search for and jam signals automatically over a broad frequency band employing a sweeping technique.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:aba786cb-5b99-4014-b948-0845e14f9773</app:SemanticID>
            <app:DisplayName>Jam, sweep</app:DisplayName>
            <app:PhysicalName>JAMSWP</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="SweepLockOnJam">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The employment of a sweeping receiver with a jammer that locks on a detected victim signal and performs spot jamming.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:a3577c57-7801-43a3-99ab-6b217c73be9e</app:SemanticID>
            <app:DisplayName>Jam, sweep lock-on</app:DisplayName>
            <app:PhysicalName>JAMSWL</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="TacticalMovementsSimulation">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A combination of sonic deception and surface craft movements during low visibility.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:43373ca5-0987-400c-96bb-c1833b7ad505</app:SemanticID>
            <app:DisplayName>Simulation, tactical movements</app:DisplayName>
            <app:PhysicalName>SIMTAC</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="TransponderJam">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Automatically transmits a predetermined signal and is programmed to respond to a specific type of victim signal.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:2dea061b-8d9b-478e-b85a-116ad84eaeb4</app:SemanticID>
            <app:DisplayName>Jam, transponder</app:DisplayName>
            <app:PhysicalName>JAMTRS</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="UnitSimulation">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Establishing a network of communications and non-communications emitters to simulate a unit or organisation. The creation of fictitious friendly units with actual or specially designed equipment.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:91cd6540-ac0e-430c-8b43-3abf709934e2</app:SemanticID>
            <app:DisplayName>Simulation, unit</app:DisplayName>
            <app:PhysicalName>SIMUNT</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
