  <xsd:element name="MineCountermeasure" type="action:MineCountermeasureType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">A Task to prevent or reduce damage or danger from mines.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:7036ce77-75f7-4bf1-b298-1d69e09718d3</app:SemanticID>
        <app:Geometry app:semanticID="MIM:1b27d622-6f58-406d-aceb-2402af195155">AbsolutePoint</app:Geometry>
        <app:DisplayName>Mine countermeasure</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MineCountermeasureCategoryCode" type="action:MineCountermeasureCategoryCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The class of MineCountermeasure.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:3fd7f8fa-67e1-4afc-b26f-9e082ffd18d7</app:SemanticID>
        <app:ShortName>categoryCode</app:ShortName>
        <app:MIMStereotype>code</app:MIMStereotype>
        <app:MIMStereotype>discriminator</app:MIMStereotype>
        <app:MIMStereotype>instance</app:MIMStereotype>
        <app:DisplayName>category</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="MineCountermeasureCategoryCodeType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The class of MineCountermeasure.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:312d9a32-3231-4d51-b0eb-cb079229614b</app:SemanticID>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="primitives:CodeValueType">
      <xsd:enumeration value="AttritionMCM">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">To conduct continuous application of Mine Countermeasures (MCM) to keep the risk from mines to all vessels as low as possible. Appropriate to minefields that are being replenished.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:700d21b0-ad31-4ca7-a03e-84ad69de2a97</app:SemanticID>
            <app:Geometry app:semanticID="MIM:1b27d622-6f58-406d-aceb-2402af195155" app:inheritedFrom="MIM:7036ce77-75f7-4bf1-b298-1d69e09718d3">AbsolutePoint</app:Geometry>
            <app:DisplayName>MCM, attrition</app:DisplayName>
            <app:PhysicalName>MCMATT</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="CheckMCM">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">To conduct a Mine Countermeasures (MCM) operation to check that as far as possible no maritime mines are left after the previous MCM operation.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:389f60ae-cea9-413d-8149-5ec4620fedc5</app:SemanticID>
            <app:Geometry app:semanticID="MIM:1b27d622-6f58-406d-aceb-2402af195155" app:inheritedFrom="MIM:7036ce77-75f7-4bf1-b298-1d69e09718d3">AbsolutePoint</app:Geometry>
            <app:DisplayName>MCM, check</app:DisplayName>
            <app:PhysicalName>MCMCHK</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ExploratoryMCM">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">To conduct a Mine Countermeasures (MCM) operation in which a sample of the route or area is subjected to MCM procedures to determine the presence or absence of mines.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:ad6b0dfc-bfbb-48ed-84c0-0922c036d790</app:SemanticID>
