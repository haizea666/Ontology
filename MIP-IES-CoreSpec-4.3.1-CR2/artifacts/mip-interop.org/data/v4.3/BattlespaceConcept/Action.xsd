            <app:PhysicalName>LAND</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="LowAltitudeExtractionParachute">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">To deplane from the aircraft while it is airborne with a parachute that will pull the cargo out of the aircraft.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:18af0d19-8519-4f47-bf08-f4b8fb307d83</app:SemanticID>
            <app:DisplayName>Parachute, low altitude extraction</app:DisplayName>
            <app:PhysicalName>PARLAE</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
