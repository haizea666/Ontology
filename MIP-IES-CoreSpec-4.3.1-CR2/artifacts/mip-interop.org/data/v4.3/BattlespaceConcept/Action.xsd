            <app:SemanticID>MIM:bd69eef6-f7ad-4fef-a3c8-4960ab6106ed</app:SemanticID>
            <app:DisplayName>As soon as possible not later than</app:DisplayName>
            <app:PhysicalName>ASAPNL</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="At">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Time intended is the time specified.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:566e77b0-1e3d-4f26-9544-0052940ca49d</app:SemanticID>
