          <xsd:documentation xml:lang="en">The Action resulted in complete disorder with respect to the object or class that is the focus of the ActionEffect.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:a161e8b7-2522-494d-98e5-e33536d2b2d0</app:SemanticID>
            <app:DisplayName>Total disruption</app:DisplayName>
            <app:PhysicalName>TOTDSR</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
