          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ModerateDamage">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The physical effect of an Action on buildings, infrastructure and/or equipment is of medium intensity.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:4d4ce4a9-d90e-4d4e-84bc-304418a13d9f</app:SemanticID>
            <app:DisplayName>Moderate damage</app:DisplayName>
            <app:PhysicalName>MODDAM</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Neutralized">
