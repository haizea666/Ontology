        <xsd:annotation>
          <xsd:documentation xml:lang="en">To deplane by jumping out of the aircraft while it is airborne and manually deploy a parachute before reaching the ground.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:4c65e41b-6d23-4ee2-99db-dd146550ebef</app:SemanticID>
            <app:DisplayName>Parachute, manual</app:DisplayName>
            <app:PhysicalName>PARMAN</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Parachute">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">To deplane by jumping out of the aircraft while it is airborne and deploy a parachute before reaching the ground.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:5b53d2f5-8aa3-445d-9c20-4bdc2fa77405</app:SemanticID>
            <app:DisplayName>Parachute</app:DisplayName>
            <app:PhysicalName>PARCHT</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Paraglider">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">To deplane by jumping out of the aircraft while it is airborne and deploy a paraglider before reaching the ground.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:86f7b583-43cb-4966-af6c-084562e53ce5</app:SemanticID>
            <app:DisplayName>Paraglider</app:DisplayName>
            <app:PhysicalName>PARGLD</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Rope">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">To deplane using a rope while the aircraft is airborne.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:e84c2579-8356-41e3-badb-d98a2eb6f151</app:SemanticID>
            <app:DisplayName>Rope</app:DisplayName>
            <app:PhysicalName>ROPE</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Wedge">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">To deplane cargo from the aircraft by dropping it, using a wedge system.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:ba297b1d-3e37-4193-a07e-08f717090830</app:SemanticID>
            <app:DisplayName>Wedge</app:DisplayName>
            <app:PhysicalName>WEDGE</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Winch">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">To deplane by using a winch-operated cable while the aircraft is airborne.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:23e168c7-8ca7-4851-a211-5fa6acdeefd7</app:SemanticID>
            <app:DisplayName>Winch</app:DisplayName>
            <app:PhysicalName>WINCH</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
