            <app:SemanticID>MIM:2ef9cdd1-b9bb-4974-b921-6c6d673f455e</app:SemanticID>
            <app:DisplayName>Neutralized</app:DisplayName>
            <app:PhysicalName>NUTRLD</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="NoResistance">
