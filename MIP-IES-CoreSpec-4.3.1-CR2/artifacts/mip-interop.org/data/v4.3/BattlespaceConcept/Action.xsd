      <xsd:enumeration value="Primary">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">This is the initial IED employed in the target area.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:09fd3e4e-a92d-4851-89a8-90cfe28a73b9</app:SemanticID>
