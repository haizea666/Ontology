          <xsd:documentation xml:lang="en">This is an additional device emplaced in the target area to attack individuals or vehicles after the initial and secondary events.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:ee2cd1fa-2294-4fff-96be-96e53501147e</app:SemanticID>
            <app:DisplayName>Tertiary</app:DisplayName>
            <app:PhysicalName>TER</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
