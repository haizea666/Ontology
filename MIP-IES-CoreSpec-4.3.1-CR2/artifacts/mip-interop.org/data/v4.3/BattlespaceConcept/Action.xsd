  <xsd:complexType name="IEDEventType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">An Event involving an Improvised Explosive Device (IED).</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:75b32b51-38c9-4ea2-950d-025e787b3f74</app:SemanticID>
        <app:Geometry app:semanticID="MIM:1b27d622-6f58-406d-aceb-2402af195155">AbsolutePoint</app:Geometry>
        <app:DisplayName>IED event</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="action:EventType">
        <xsd:sequence>
          <xsd:element ref="action:IEDEventEmplacementCode" minOccurs="0" />
          <xsd:element ref="action:IEDEventEmploymentMethodCode" minOccurs="0" />
          <xsd:element ref="action:IEDEventIncidentCode" />
