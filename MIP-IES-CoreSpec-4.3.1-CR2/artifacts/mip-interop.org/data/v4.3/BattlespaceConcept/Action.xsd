            <app:SemanticID>MIM:c07edee8-145b-482b-af7c-0fd159df6446</app:SemanticID>
            <app:DisplayName>Anti-personnel</app:DisplayName>
            <app:PhysicalName>PERDEF</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AntiVehicle">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The intended result is to damage or destroy vehicles, excluding armoured vehicles, and/or their cargo as well as to kill or wound individuals inside such vehicles.</xsd:documentation>
