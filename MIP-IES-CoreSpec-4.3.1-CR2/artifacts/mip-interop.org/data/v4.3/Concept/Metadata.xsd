  <xsd:element name="CorrelationCategoryCode" type="conceptmetadata:CorrelationCategoryCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The type of Correlation.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:04b97d23-cf70-4460-9668-6edd1f36d6a4</app:SemanticID>
        <app:ShortName>categoryCode</app:ShortName>
        <app:ValueOrAmplification>1</app:ValueOrAmplification>
