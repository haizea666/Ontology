  <xsd:simpleType name="IntelAppraisalReliabilityCodeType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The general appraisal of the source, for intelligence purpose, in graded terms to indicate the extent to which it has been proven it can be counted on or trusted in to do as expected.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:aa11ffe2-0fbf-4130-a040-b0fc213f5dcc</app:SemanticID>
        <app:MIMStereotype>complete</app:MIMStereotype>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="primitives:CodeValueType">
      <xsd:enumeration value="CompletelyReliable">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The source of the reported data can be considered as completely reliable, i.e. erroneous information cannot be produced.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:d71ed023-3b74-439d-a170-2ea180816245</app:SemanticID>
            <app:DisplayName>Completely reliable</app:DisplayName>
            <app:PhysicalName>A</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="FairlyReliable">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The source of the reported data can be considered as fairly reliable, i.e. erroneous information can be produced, with a probability that cannot be neglected.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:8a3dddf5-ecf3-4a91-9e87-d88ecf76bc49</app:SemanticID>
            <app:DisplayName>Fairly reliable</app:DisplayName>
            <app:PhysicalName>C</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="NotUsuallyReliable">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The source of the reported data is not usually reliable, i.e. the probability of producing erroneous information is high (&gt;30%).</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:a82446b9-22cc-4fe8-a636-27c4a286cc1e</app:SemanticID>
            <app:DisplayName>Not usually reliable</app:DisplayName>
            <app:PhysicalName>D</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ReliabilityCannotBeJudged">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The reliability of the source of the reported data cannot be estimated.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:1626f80c-f5bc-44d4-83d0-8b0e89dcc96e</app:SemanticID>
            <app:DisplayName>Reliability cannot be judged</app:DisplayName>
            <app:PhysicalName>F</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Unreliable">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The source of the reported data is not reliable, i.e. the produced reported data can generally be considered as erroneous.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:96024183-e2b3-4574-bf6a-2f228a2ad5ee</app:SemanticID>
            <app:DisplayName>Unreliable</app:DisplayName>
            <app:PhysicalName>E</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="UsuallyReliable">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The source of the reported data can be considered as usually reliable, i.e. erroneous information can be produced, but the probability of such events can be neglected.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:3683b72e-7a30-4173-8677-1958b778a9b6</app:SemanticID>
            <app:DisplayName>Usually reliable</app:DisplayName>
            <app:PhysicalName>B</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
