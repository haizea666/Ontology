  <xsd:element name="ConceptMetadataAlternativeConfidentialityLabel" type="confidentialitylabel:ConfidentialityLabelType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">An equivalent representation, in an alternative policy, of the originator confidentiality label.</xsd:documentation>
