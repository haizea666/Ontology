        <app:MIMStereotype>instance</app:MIMStereotype>
        <app:DisplayName>category</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="CorrelationCategoryCodeType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The type of Correlation.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:3bd837cd-7a00-4a34-8fcf-ecec811db3bb</app:SemanticID>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="primitives:CodeValueType">
      <xsd:enumeration value="Confirms">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The information associated with the Correlation affirms the information given in the InformationGroup.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:356b76af-d888-416a-9b61-ee59e7dfbcfd</app:SemanticID>
            <app:DisplayName>confirms</app:DisplayName>
            <app:PhysicalName>CONF</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Corrects">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The information associated with the Correlation amends the information given in the InformationGroup.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:6b3dcb02-d51b-4f4c-9b31-34a96d7540a1</app:SemanticID>
            <app:DisplayName>corrects</app:DisplayName>
            <app:PhysicalName>CORR</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Defines">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The information associated with the Correlation defines the information given in the InformationGroup.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:8ad4d53b-ac3f-4841-86ec-da71688d226b</app:SemanticID>
            <app:DisplayName>defines</app:DisplayName>
            <app:PhysicalName>ISDFT</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="IsImpliedBy">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The information associated with the Correlation is the logical consequence of the information given in the InformationGroup.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:857e4404-af75-44cb-8a09-7e4462e2fb71</app:SemanticID>
            <app:DisplayName>is implied by</app:DisplayName>
            <app:PhysicalName>IMPL</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Negates">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The information associated with the Correlation is in some way contradictory to the information given in the InformationGroup.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:b3dc7d4e-ddd8-4d3f-afe0-a248519eaf97</app:SemanticID>
            <app:DisplayName>negates</app:DisplayName>
            <app:PhysicalName>NEGA</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Supersedes">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The information associated with the Correlation replaces the information given in the InformationGroup.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:ca52342a-c1c8-4eb3-908a-c7fd0f9b88c1</app:SemanticID>
            <app:DisplayName>supersedes</app:DisplayName>
            <app:PhysicalName>SUPR</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
