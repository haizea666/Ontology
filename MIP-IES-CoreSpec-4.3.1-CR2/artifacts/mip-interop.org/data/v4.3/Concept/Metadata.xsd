  <xsd:element name="IntelAppraisalAccuracyCode" type="conceptmetadata:IntelAppraisalAccuracyCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">For intelligence purpose, the general appraisal of the subject matter in graded terms to indicate the extent or degree to which it has been judged to be free from mistake or error or to conform to truth or some recognised standard value.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:8a525ac0-d4c9-4c09-be20-d66a98333b26</app:SemanticID>
        <app:ShortName>accuracyCode</app:ShortName>
        <app:ValueOrAmplification>1</app:ValueOrAmplification>
        <app:MIMStereotype>code</app:MIMStereotype>
        <app:MIMStereotype>instance</app:MIMStereotype>
        <app:DisplayName>accuracy</app:DisplayName>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="IntelAppraisalAccuracyCodeType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">The general appraisal, for intelligence purpose, of the subject matter in graded terms to indicate the extent or degree to which it has been judged to be free from mistake or error or to conform to truth or some recognised standard value.</xsd:documentation>
      <xsd:appinfo>
        <app:SemanticID>MIM:c0dec873-a722-4086-bb3c-a4edc0e66b36</app:SemanticID>
        <app:MIMStereotype>complete</app:MIMStereotype>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="primitives:CodeValueType">
      <xsd:enumeration value="Confirmed">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Reported data is confirmed by at least 3 independent sources.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:2ff66049-29f5-461c-8f48-6c4281b7b3a6</app:SemanticID>
            <app:DisplayName>Confirmed</app:DisplayName>
            <app:PhysicalName>1</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Doubtful">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Reported data can be viewed with suspicion.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:ebe1a368-1314-4699-abaf-9fb2ff19e12b</app:SemanticID>
            <app:DisplayName>Doubtful</app:DisplayName>
            <app:PhysicalName>4</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Improbable">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Reported data shall be considered as probably erroneous.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:9fc0b076-fdce-4154-8617-cb7d7f4f8b9c</app:SemanticID>
            <app:DisplayName>Improbable</app:DisplayName>
            <app:PhysicalName>5</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Possible">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Reported data is given by only one source.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:1e9656ec-f229-425a-a9a8-5ecfcec4e969</app:SemanticID>
            <app:DisplayName>Possible</app:DisplayName>
            <app:PhysicalName>3</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Probable">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Reported data is confirmed by 2 independent sources.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:33ea56e1-e130-45d7-829c-db9ff5c09e18</app:SemanticID>
            <app:DisplayName>Probable</app:DisplayName>
            <app:PhysicalName>2</app:PhysicalName>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="TruthCannotBeJudged">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Basis for the estimate of Reported data cannot be estimated.</xsd:documentation>
          <xsd:appinfo>
            <app:SemanticID>MIM:67effffb-ad3c-4522-886a-7cdd45dad1a3</app:SemanticID>
            <app:DisplayName>Truth cannot be judged</app:DisplayName>
